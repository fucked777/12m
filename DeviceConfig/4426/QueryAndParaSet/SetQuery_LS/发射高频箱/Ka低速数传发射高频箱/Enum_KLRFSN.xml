<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="KLRFSN" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="DeviceStatus" desc="设备状态">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="故障:R"/>
		<entry svalue="2" uvalue="2" desc="正常:G"/>
	</enum>
	<enum name="PowerStatus" desc="电源状态">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="电源开:G"/>
		<entry svalue="2" uvalue="2" desc="电源关:Y"/>
	</enum>
	<enum name="KaLanchSwitchNet" desc="Ka发射射频开关网络">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="Ka数传U/C1"/>
		<entry svalue="2" uvalue="2" desc="Ka数传U/C2"/>
	</enum>
	<enum name="LOSwitchNet" desc="本振开关网络">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="上变频1本振"/>
		<entry svalue="2" uvalue="2" desc="上变频2本振"/>
		<entry svalue="3" uvalue="3" desc="小环下变频本振"/>
	</enum>
	<enum name="DownChangeFreqSwitchNet" desc="下变频开关网络">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="功放小环"/>
		<entry svalue="2" uvalue="2" desc="变频小环"/>
	</enum>
</enums>
