		<field pid="7" GroupId="status4" id="LedStatus12004" desc="第4路" dataType="uint8" displayFormat="LED|DevStatus" minValue="" maxValue="" step="" unit=""/>
		<field pid="8" GroupId="status2" id="LedStatus2018Send" desc="2018~2310" dataType="uint8" displayFormat="LED|DevStatus" minValue="" maxValue="" step="" unit=""/>
		<field pid="9" GroupId="status2" id="LedStatus2200" desc="2200~2310" dataType="uint8" displayFormat="LED|DevStatus" minValue="" maxValue="" step="" unit=""/>
