<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="TCU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="CommState" desc="通信状态">
		<entry svalue="1" uvalue="1" desc="通:G"/>
		<entry svalue="2" uvalue="2" desc="断:R"/>
	</enum>
</enums>
