<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="KTTAS" ModeType="0x09" edition="1.1.1.1">
	<enum name="MoniyuanSource" desc="模拟源选择">
		<entry svalue="1" uvalue="1" desc="遥测模拟源"/>
		<entry svalue="2" uvalue="2" desc="遥控模拟源"/>
	</enum>
	<enum name="DataSource" desc="数据源选择">
		<entry svalue="1" uvalue="1" desc="内部"/>
		<entry svalue="2" uvalue="2" desc="文件(含外部文件)"/>
	</enum>
	<enum name="DataType" desc="数据类型">
		<entry svalue="1" uvalue="1" desc="固定数"/>
		<entry svalue="2" uvalue="2" desc="随机数"/>
	</enum>
</enums>
