<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="TTCM" ModeType="0x01" edition="1.1.1.1">
	<enum name="CarrierOutput" desc="载波输出">
		<entry svalue="1" uvalue="1" desc="允许输出"/>
		<entry svalue="2" uvalue="2" desc="禁止输出"/>
	</enum>
	<enum name="DataModulSwitch" desc="数据调制开关">
		<entry svalue="1" uvalue="1" desc="加调"/>
		<entry svalue="2" uvalue="2" desc="去调"/>
	</enum>
	<enum name="RangPlusCtrl" desc="侧音调制控制">
		<entry svalue="1" uvalue="1" desc="加调"/>
		<entry svalue="2" uvalue="2" desc="去调"/>
	</enum>
	<enum name="RanPlusCtrl" desc="编遥调制控制">
		<entry svalue="1" uvalue="1" desc="加调"/>
		<entry svalue="2" uvalue="2" desc="去调"/>
	</enum>
	<enum name="NoiseOutput" desc="噪声源开关">
		<entry svalue="1" uvalue="1" desc="关"/>
		<entry svalue="2" uvalue="2" desc="开"/>
	</enum>
	<enum name="WorkingSystem" desc="转发模式">
		<entry svalue="1" uvalue="1" desc="相干"/>
		<entry svalue="2" uvalue="2" desc="非相干"/>
	</enum>
</enums>
