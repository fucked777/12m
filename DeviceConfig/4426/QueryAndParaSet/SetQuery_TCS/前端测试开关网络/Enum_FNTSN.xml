<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="FNTSN" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="DevStatus" desc="设备状态">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="故障:R"/>
		<entry svalue="2" uvalue="2" desc="正常:G"/>
	</enum>
	<enum name="PowerStatus" desc="设备开关电">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="关电:Y"/>
		<entry svalue="2" uvalue="2" desc="开电:G"/>
	</enum>
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="0" uvalue="0" desc="无效"/>
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="SFreqSwitch" desc="S频段选择开关">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="S射频模拟源"/>
		<entry svalue="2" uvalue="2" desc="前端信号源输入"/>
	</enum>
	<enum name="XFreqSwitch" desc="X频段选择开关">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="X射频模拟源"/>
		<entry svalue="2" uvalue="2" desc="前端信号源输入"/>
	</enum>
	<enum name="KaFreqSwitch" desc="Ka频段开关选择">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="Ka低速数传模拟源"/>
		<entry svalue="2" uvalue="2" desc="Ka高速数传模拟源"/>
		<entry svalue="3" uvalue="3" desc="Ka应答机下行入"/>
		<entry svalue="4" uvalue="4" desc="前端信号源输入"/>
	</enum>
	<enum name="CommonSwitch" desc="开关选择">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="打开"/>
		<entry svalue="2" uvalue="2" desc="关闭"/>
	</enum>
	<enum name="SFreqSwitchS" desc="S频段选择开关">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="S射频模拟源"/>
		<entry svalue="2" uvalue="2" desc="前端信号源输入"/>
	</enum>
	<enum name="XFreqSwitchS" desc="X频段选择开关">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="X射频模拟源"/>
		<entry svalue="2" uvalue="2" desc="前端信号源输入"/>
	</enum>
	<enum name="KaFreqSwitchS" desc="Ka频段开关选择">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="Ka模拟源1入(测控/低速)"/>
		<entry svalue="2" uvalue="2" desc="Ka模拟源2入(高速)"/>
		<entry svalue="3" uvalue="3" desc="Ka应答机下行入"/>
		<entry svalue="4" uvalue="4" desc="前端信号源输入"/>
	</enum>
</enums>
