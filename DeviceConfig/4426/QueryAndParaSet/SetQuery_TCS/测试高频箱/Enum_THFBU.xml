<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="THFBU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="0" uvalue="0" desc="无效"/>
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="InterExterRefe" desc="内外参考">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="内参考"/>
		<entry svalue="2" uvalue="2" desc="外参考"/>
	</enum>
	<enum name="XLocalLockSource" desc="X模拟源本振锁定">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="锁定:G"/>
		<entry svalue="2" uvalue="2" desc="失锁:R"/>
	</enum>
	<enum name="KLocalLockSource" desc="Ka模拟源本振锁定">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="锁定:G"/>
		<entry svalue="2" uvalue="2" desc="失锁:R"/>
	</enum>
	<enum name="RefeSourceLock" desc="参考源锁定状态">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="锁定:G"/>
		<entry svalue="2" uvalue="2" desc="失锁:R"/>
	</enum>
	<enum name="DeviceStatus" desc="设备状态">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="故障:R"/>
		<entry svalue="2" uvalue="2" desc="正常:G"/>
	</enum>
	<enum name="EquipPlusCtrl" desc="设备加去电控制">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="关电"/>
		<entry svalue="2" uvalue="2" desc="开电"/>
	</enum>
	<enum name="SignalOutput" desc="信号源输出选择">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<!-- <entry svalue="1" uvalue="1" desc="S测试" /> 保留 -->
		<entry svalue="2" uvalue="2" desc="X低速"/>
		<entry svalue="3" uvalue="3" desc="Ka测控/低速"/>
	</enum>
	<enum name="STestSelect" desc="S测试选择">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="S自检"/>
		<entry svalue="2" uvalue="2" desc="信号源"/>
	</enum>
	<enum name="DeDiffCoup" desc="去差耦合">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="通"/>
		<entry svalue="2" uvalue="2" desc="断"/>
	</enum>
	<enum name="GotoSwitch" desc="去S和开关">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="通"/>
		<entry svalue="2" uvalue="2" desc="断"/>
	</enum>
	<enum name="XTestSelect" desc="X测试选择">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="X自检"/>
		<entry svalue="2" uvalue="2" desc="信号源"/>
	</enum>
	<enum name="KaTestSelect" desc="Ka测试选择">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="Ka自检"/>
