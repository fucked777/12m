	</enum>
	<enum name="BSSelect" desc="标校天线">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="通"/>
		<entry svalue="2" uvalue="2" desc="断"/>
	</enum>
</enums>
