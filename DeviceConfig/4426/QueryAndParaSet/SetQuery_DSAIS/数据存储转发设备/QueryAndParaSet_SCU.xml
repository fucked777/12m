<?xml version="1.0" encoding="UTF-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
set: 参数设置 ,result:状态结果上报
name:为字段名,对应相应设备参数xml文件中的参数名
fieldType:(code:命令码,data:数据,const:固定值)
dataType:数据类型(char,int8,uint8,short,ushort,int,uint,float,double,long,ulong,char|n)
displayFormat:界面显示类型(char,Int8,UInt8,Int16,UInt16,Int32,UInt32,float,double,Int64,UInt64,String,Bit,IP)
dataType中char|n:字符数组长度,如char|6
displayFormat中IP:按IP地址显示
displayFormat中Bit:按位进行显示
UnitType:单元类型
ModeType:模式类型
initValue:初始化值
recycle:循环  id表示循环次数的字段名  <recycle id="TargetNum"></recycle>  出现多个时,在名称前面加数字进行排序,如1TargetNum
isShow:0表示界面不显示  1表示界面显示
isControl:0表示状态 1表示设置
isAlarm:是否告警 0表示不告警 1表示告警
BitDivision:位取值BitDivision="1|2|3|5";如uint32为例,每1位都表示一个内容BitDivision="1";每2位都表示一个内容BitDivision="2";
每一个都不同时则用|分开,如BitDivision="1|2|3|5",1表示第一个为1位,2表示第二个紧跟着的2-3位的值,3表示第二个紧跟着的4-6位的值,5后面不写全表示5后面的字段保留;
-->
<cmd UnitType="SCU" ModeType="0xFFFF" desc="分系统公共单元" flag="1001" isModifySetBtn="0">
	<result MessageType="0x0000F104">
		<field pid="1" GroupId="Common" id="ControlMode" desc="监控方式" dataType="uint8" displayFormat="enum|MonitMode" initValue="" minValue="" maxValue="" step="" unit=""/>
		<field pid="2" GroupId="Common" id="WorkStatus" desc="工作状态" dataType="uint8" displayFormat="LED|WorkStatus" initValue="" minValue="" maxValue="" step="" unit=""/>
		<field pid="3" GroupId="Save" id="ReceNumb" desc="接收存储路数" dataType="uint8" displayFormat="UInt8" initValue="" minValue="" maxValue="" step="" unit=""/>
		<field pid="4" GroupId="Save" id="RouteNumb" desc="发送路数" dataType="uint8" displayFormat="UInt8" initValue="" minValue="" maxValue="" step="" unit=""/>
		<field pid="5" GroupId="NetSave" id="NetDiskSpace" desc="磁盘空间状态" dataType="uint8" displayFormat="enum|DiskSpace" initValue="" minValue="" maxValue="" step="" unit=""/>
		<field pid="6" GroupId="NetSave" id="NetTotalDisk" desc="记录盘总容量" dataType="uint" displayFormat="UInt32" initValue="" minValue="" maxValue="" step="" unit="MB"/>
		<field pid="7" GroupId="NetSave" id="CapUseRecord" desc="已用记录盘容量" dataType="uint" displayFormat="UInt32" initValue="" minValue="" maxValue="" step="" unit="MB"/>
		<field pid="8" GroupId="NetSave" id="RedunCapacit" desc="空余记录盘容量" dataType="uint" displayFormat="UInt32" initValue="" minValue="" maxValue="" step="" unit="MB"/>
		<field pid="9" GroupId="Common" id="TotalSend" desc="发送总速率" dataType="ulong" displayFormat="UInt64" initValue="" minValue="" maxValue="" step="" unit="bps"/>
		<field pid="10" GroupId="Common" id="TotalRece" desc="接收总速率" dataType="ulong" displayFormat="UInt64" initValue="" minValue="" maxValue="" step="" unit="bps"/>
		<field pid="11" GroupId="LocalSave" id="LocalDiskSpace" desc="磁盘空间状态" dataType="uint8" displayFormat="enum|DiskSpace" initValue="" minValue="" maxValue="" step="" unit=""/>
		<field pid="12" GroupId="LocalSave" id="TotalDiskCapa" desc="记录盘总容量" dataType="uint" displayFormat="UInt32" initValue="" minValue="" maxValue="" step="" unit="MB"/>
		<field pid="13" GroupId="LocalSave" id="UsedDiskCapa" desc="已用记录盘容量" dataType="uint" displayFormat="UInt32" initValue="" minValue="" maxValue="" step="" unit="MB"/>
		<field pid="14" GroupId="LocalSave" id="RedDiskCapa" desc="空余记录盘容量" dataType="uint" displayFormat="UInt32" initValue="" minValue="" maxValue="" step="" unit="MB"/>
		<field pid="15" GroupId="Save" id="CUPUsage" desc="CPU使用率" dataType="ushort" displayFormat="double" initValue="" minValue="1" maxValue="1000" step="0.1" unit="%"/>
		<field pid="16" GroupId="Save" id="NemoryUtil" desc="内存使用率" dataType="ushort" displayFormat="double" initValue="" minValue="1" maxValue="1000" step="0.1" unit="%"/>
		<field pid="17" GroupId="Save" id="StorSpace" desc="正使用存储空间" dataType="uint8" displayFormat="enum|StorSpace" initValue="" minValue="" maxValue="" step="" unit=""/>
		<field pid="18" GroupId="Save" id="AndNetwork" desc="网络链路状态" dataType="uint8" displayFormat="LED|AndNetwork" initValue="" minValue="" maxValue="" step="" unit=""/>
	</result>
	<layout>
		<item id="Common" desc="公共参数" row="0" col="0" rowspan="1" colspan="2"/>
		<item id="Save" desc="存储" row="1" col="0" rowspan="1" colspan="1"/>
		<item id="NetSave" desc="网络存储" row="1" col="1" rowspan="1" colspan="1"/>
		<item id="LocalSave" desc="本地存储" row="2" col="0" rowspan="1" colspan="2"/>
	</layout>
</cmd>
