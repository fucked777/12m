<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="MACDFELU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="SendSwitch" desc="发射开关">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="禁止"/>
		<entry svalue="2" uvalue="2" desc="输出"/>
	</enum>
	<enum name="SendSource" desc="发射来源">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="基带"/>
		<entry svalue="2" uvalue="2" desc="前端"/>
	</enum>
	<enum name="ConnectStatus" desc="连接状态">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="已连接:G"/>
		<entry svalue="2" uvalue="2" desc="未连接:R"/>
		<entry svalue="3" uvalue="3" desc="故障:R"/>
	</enum>
</enums>
