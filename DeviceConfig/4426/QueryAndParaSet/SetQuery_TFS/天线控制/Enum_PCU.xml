<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="PCU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="PolarCtrlStatus" desc="极化控制器状态">
		<entry svalue="0" uvalue="0" desc="无此参数:Y"/>
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="告警:R"/>
	</enum>
	<enum name="MonitorMode" desc="监控方式">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="ACU控"/>
		<entry svalue="2" uvalue="2" desc="极化器控"/>
	</enum>
	<enum name="EmissPolar" desc="发射极化">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="左旋圆极化"/>
		<entry svalue="2" uvalue="2" desc="右旋圆极化"/>
	</enum>
	<enum name="ReceivPolar" desc="接收极化">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="左旋圆极化"/>
		<entry svalue="2" uvalue="2" desc="右旋圆极化"/>
	</enum>
	<enum name="TrackPolar" desc="跟踪极化">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="左旋圆极化"/>
		<entry svalue="2" uvalue="2" desc="右旋圆极化"/>
	</enum>
	<enum name="SPolar" desc="S发射极化">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="左旋圆极化"/>
		<entry svalue="2" uvalue="2" desc="右旋圆极化"/>
	</enum>
	<enum name="SRecPolar" desc="S接收极化">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="左旋圆极化"/>
		<entry svalue="2" uvalue="2" desc="右旋圆极化"/>
	</enum>
</enums>
