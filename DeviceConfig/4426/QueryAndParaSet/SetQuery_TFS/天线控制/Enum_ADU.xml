	</enum>
	<enum name="AZDirverState" desc="方位驱动器状态">
		<entry svalue="1" uvalue="1" desc="故障:R"/>
		<entry svalue="2" uvalue="2" desc="正常:G"/>
	</enum>
	<enum name="LightnProtConn" desc="避雷电缆接通">
		<entry svalue="1" uvalue="1" desc="接通:G"/>
