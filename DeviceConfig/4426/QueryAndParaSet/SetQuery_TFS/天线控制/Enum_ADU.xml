<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="ADU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="AZMonitMode" desc="方位驱动监控方式">
		<entry svalue="1" uvalue="1" desc="ACU控"/>
		<entry svalue="2" uvalue="2" desc="驱动柜控"/>
	</enum>
	<enum name="AZDirvState" desc="方位驱动状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="告警:R"/>
	</enum>
	<enum name="AZDirvPower" desc="方位驱动器加电">
		<entry svalue="1" uvalue="1" desc="加电"/>
		<entry svalue="2" uvalue="2" desc="去电"/>
	</enum>
	<enum name="AZunLockPlace" desc="方位拔锁到位">
		<entry svalue="1" uvalue="1" desc="到位"/>
		<entry svalue="2" uvalue="2" desc="不到位"/>
	</enum>
