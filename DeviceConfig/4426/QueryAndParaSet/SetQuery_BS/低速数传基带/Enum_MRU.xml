<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="MRU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="CarrierLock" desc="载波锁定指示">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="锁定:G"/>
		<entry svalue="2" uvalue="2" desc="失锁:R"/>
	</enum>
	<enum name="AGUTime" desc="AGC时间常数">
		<entry svalue="1" uvalue="1" desc="1ms"/>
		<entry svalue="2" uvalue="2" desc="10ms"/>
		<entry svalue="3" uvalue="3" desc="100ms"/>
		<entry svalue="4" uvalue="4" desc="1000ms"/>
	</enum>
	<enum name="CarrierModulation" desc="载波调制方式">
		<entry svalue="1" uvalue="1" desc="BPSK"/>
		<entry svalue="2" uvalue="2" desc="QPSK"/>
		<!--<entry svalue="3" uvalue="3" desc="SQPSK" />-->
		<entry svalue="4" uvalue="4" desc="UQPSK"/>
		<entry svalue="5" uvalue="5" desc="OQPSK"/>
		<!--<entry svalue="6" uvalue="6" desc="GMSK" />-->
	</enum>
	<enum name="ParallelSCMode" desc="并串转换方式">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="按比特"/>
		<entry svalue="2" uvalue="2" desc="按帧"/>
	</enum>
	<enum name="SignalModelChoose" desc="信号模式选择">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="I路独立或I/Q合路"/>
		<entry svalue="2" uvalue="2" desc="I/Q独立"/>
	</enum>
	<enum name="GrayCode" desc="QPSK调制Gray编码选择">
		<entry svalue="1" uvalue="1" desc="Gray0编码"/>
		<entry svalue="2" uvalue="2" desc="Gray1编码"/>
		<entry svalue="3" uvalue="3" desc="Gray2编码"/>
		<entry svalue="4" uvalue="4" desc="Gray3编码"/>
		<entry svalue="5" uvalue="5" desc="Gray4编码"/>
		<entry svalue="6" uvalue="6" desc="Gray5编码"/>
		<entry svalue="7" uvalue="7" desc="Gray6编码"/>
		<entry svalue="8" uvalue="8" desc="Gray7编码"/>
		<entry svalue="9" uvalue="9" desc="无格雷编码"/>
		<entry svalue="10" uvalue="10" desc="自适应"/>
	</enum>
	<enum name="PhaseMapping" desc="相位映射">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="I+Q"/>
		<entry svalue="2" uvalue="2" desc="I-Q"/>
		<entry svalue="3" uvalue="3" desc="自适应"/>
	</enum>
	<enum name="InputInterface" desc="输入信号接口">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="本板信号输入"/>
		<entry svalue="2" uvalue="2" desc="板间信号输入"/>
	</enum>
	<enum name="PolarizationMode" desc="极化方式">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="左旋"/>
		<entry svalue="2" uvalue="2" desc="右旋"/>
	</enum>
	<enum name="CarrLoopBand" desc="载波环路带宽">
		<entry svalue="1" uvalue="1" desc="100Hz"/>
		<entry svalue="2" uvalue="2" desc="300Hz"/>
		<entry svalue="3" uvalue="3" desc="1000Hz"/>
		<entry svalue="4" uvalue="4" desc="3000Hz"/>
	</enum>
	<enum name="BlockCodeCheck" desc="分组码校验输出选择">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="不输出"/>
		<entry svalue="2" uvalue="2" desc="输出"/>
	</enum>
	<enum name="DataOutWay" desc="数据输出方式">
		<entry svalue="0" uvalue="0" desc="I/Q合路"/>
		<entry svalue="1" uvalue="1" desc="I/Q分路"/>
		<entry svalue="2" uvalue="2" desc="I独立"/>
	</enum>
</enums>
