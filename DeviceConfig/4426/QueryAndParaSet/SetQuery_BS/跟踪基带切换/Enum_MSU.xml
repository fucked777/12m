<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="MSU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="DevStatus" desc="设备状态">
		<entry svalue="1" uvalue="1" desc="故障:R"/>
		<entry svalue="2" uvalue="2" desc="正常:G"/>
	</enum>
	<enum name="BandAngleOutput" desc="频段角误差选择输出">
		<entry svalue="0" uvalue="10" desc="无"/>
		<entry svalue="1" uvalue="1" desc="跟踪基带A输出"/>
		<entry svalue="2" uvalue="2" desc="跟踪基带B输出"/>
		<entry svalue="3" uvalue="3" desc="跟踪基带C输出"/>
		<entry svalue="4" uvalue="4" desc="跟踪基带D输出"/>
	</enum>
	<enum name="TestSignalOutput" desc="测试信号选择输出">
		<entry svalue="1" uvalue="1" desc="测控基带A输出"/>
		<entry svalue="2" uvalue="2" desc="测控基带B输出"/>
	</enum>
</enums>
