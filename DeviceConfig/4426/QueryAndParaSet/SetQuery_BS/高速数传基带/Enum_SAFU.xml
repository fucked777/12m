<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="SAFU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="SDWorkState" desc="工作状态">
		<entry svalue="0" uvalue="0" desc="停止"/>
		<entry svalue="1" uvalue="1" desc="记录中"/>
	</enum>
	<enum name="SendDataSwitch" desc="送数开关">
		<entry svalue="0" uvalue="0" desc="停止送数:Y"/>
		<entry svalue="1" uvalue="1" desc="开始送数:G"/>
	</enum>
	<enum name="DataTransProt" desc="数据传输协议">
		<entry svalue="0" uvalue="0" desc="UDP"/>
		<entry svalue="1" uvalue="1" desc="TCP"/>
	</enum>
</enums>
