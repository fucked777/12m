		<entry svalue="5" uvalue="5" desc="5字节"/>
		<entry svalue="6" uvalue="6" desc="6字节"/>
		<entry svalue="7" uvalue="7" desc="7字节"/>
		<entry svalue="8" uvalue="8" desc="8字节"/>
