		<entry svalue="2" uvalue="2" desc="失锁:R"/>
	</enum>
	<enum name="SmallResult" desc="小环比对结果">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
