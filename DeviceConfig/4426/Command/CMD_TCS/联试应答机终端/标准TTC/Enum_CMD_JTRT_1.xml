<?xml version="1.0" encoding="utf-8"?>
<!--
id  枚举ID  -与设备类型关联
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums ExtenType="JTRT" ModeType="0x01" edition="1.1.1.1">
	<enum name="WorkMode" desc="工作模式">
		<entry svalue="1" uvalue="1" desc="标准TTC"/>
		<entry svalue="2" uvalue="2" desc="扩频TTC"/>
	</enum>
	<enum name="WorkWay" desc="工作方式">
		<entry svalue="1" uvalue="1" desc="动态仿真"/>
		<entry svalue="2" uvalue="2" desc="文件仿真"/>
	</enum>
	<enum name="FileLoop" desc="文件循环">
		<entry svalue="1" uvalue="1" desc="单次回放"/>
		<entry svalue="2" uvalue="2" desc="循环回放"/>
	</enum>
	<enum name="CarrierOutput" desc="载波输出">
		<entry svalue="1" uvalue="1" desc="输出"/>
		<entry svalue="2" uvalue="2" desc="停止输出"/>
	</enum>
	<enum name="RangAdjust" desc="测距加调">
		<entry svalue="1" uvalue="1" desc="加调"/>
		<entry svalue="2" uvalue="2" desc="去调"/>
	</enum>
	<enum name="ModulCtrl" desc="加调控制">
		<entry svalue="1" uvalue="1" desc="加调"/>
		<entry svalue="2" uvalue="2" desc="去调"/>
	</enum>
	<enum name="SimulCtrl" desc="仿真控制">
		<entry svalue="1" uvalue="1" desc="开始"/>
		<entry svalue="2" uvalue="2" desc="停止"/>
	</enum>
	<enum name="UpInChoose" desc="输入方式">
		<!--
		<entry svalue="1" uvalue="1" desc="射频"/>
		-->
		<entry svalue="2" uvalue="2" desc="中频"/>
	</enum>
</enums>
