<?xml version="1.0" encoding="utf-8"?>
<!--
id  枚举ID  -与设备类型关联
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums ExtenType="KaHSATHB" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="DevNumber" desc="工作机号">
		<entry svalue="1" uvalue="1" desc="差路监测"/>
		<entry svalue="2" uvalue="2" desc="左和监测"/>
		<entry svalue="3" uvalue="3" desc="右和监测"/>
		<entry svalue="4" uvalue="4" desc="跟踪A组本振"/>
		<entry svalue="5" uvalue="5" desc="跟踪B组本振"/>
		<entry svalue="6" uvalue="6" desc="和路数传A本振"/>
		<entry svalue="7" uvalue="7" desc="和路数传B本振"/>
		<entry svalue="8" uvalue="8" desc="和路数传C本振"/>
		<entry svalue="9" uvalue="9" desc="和路数传D本振"/>
		<entry svalue="10" uvalue="10" desc="和路数传E本振"/>
		<entry svalue="11" uvalue="11" desc="和路数传F本振"/>
	</enum>
	<enum name="PowerControl" desc="供电开关控制">
		<entry svalue="1" uvalue="1" desc="关电"/>
		<entry svalue="2" uvalue="2" desc="开电"/>
	</enum>
	<enum name="CtrlMode" desc="控制模式">
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
</enums>
