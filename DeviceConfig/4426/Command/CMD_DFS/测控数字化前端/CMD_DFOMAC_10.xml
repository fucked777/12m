<?xml version="1.0" encoding="utf-8"?>
<!--
require:查询数据包 ,response:响应数据包,result:上报数据包
name:为字段名,对应相应设备参数xml文件中的参数名
fieldType:(code:命令码,data:数据,const:固定值)
dataType:数据类型(char,int8,uint8,short,ushort,int,uint,float,double,long,ulong,char|n)
displayFormat:界面显示类型(char,Int8,UInt8,Int16,UInt16,Int32,UInt32,float,double,Int64,UInt64,String,Bit,IP)
dataType中char|n:字符数组长度,如char|6
displayFormat中IP:按IP地址显示
displayFormat中Bit:按位进行显示
initValue为初始化值,
length为标识长度
ExtenType:分机类型
SelfLoopNumb:自身循环N次
recycle:循环  id表示循环次数的字段名  <recycle id="TargetNum"></recycle>  出现多个时,在名称前面加数字进行排序,如1TargetNum
cmdID:命令ID  与命令序号关联
isShow:0表示界面不显示  1表示界面显示
-->
<cmd cmdID="10" ExtenType="DFOMAC" ModeType="0xFFFF" desc="AGC时间常数测试" flag="1001">
	<request MessageType="0x0000F001">
		<field pid="101" id="TestCtrl" desc="测试控制" dataType="uint8" displayFormat="enum|TestCtrl" initValue="" minValue="" maxValue="" step="" unit=""/>
		<field pid="102" id="ChanReceNumb" desc="通道号" dataType="uint8" displayFormat="enum|ChanReceNumb" initValue="" minValue="" maxValue="" step="" unit="" isShow="0"/>
	</request>
	<result MessageType="0x0000F101">
		<field pid="1" id="TestState" desc="测试状态" dataType="uint8" displayFormat="enum|TestState" initValue="" minValue="" maxValue="" step="" unit=""/>
		<field pid="2" id="ChanReceNumb" desc="通道号" dataType="uint8" displayFormat="enum|ChanReceNumb" initValue="" minValue="" maxValue="" step="" unit="" isShow="0"/>
		<field pid="3" id="AGCTime1" desc="时常1ms" dataType="int" displayFormat="double" initValue="" minValue="" maxValue="" step="0.01" unit="ms"/>
		<field pid="4" id="AGCTime10" desc="时常10ms" dataType="int" displayFormat="double" initValue="" minValue="" maxValue="" step="0.01" unit="ms"/>
		<field pid="5" id="AGCTime100" desc="时常100ms" dataType="int" displayFormat="double" initValue="" minValue="" maxValue="" step="0.01" unit="ms"/>
		<field pid="6" id="AGCTime1000" desc="时常1000ms" dataType="int" displayFormat="double" initValue="" minValue="" maxValue="" step="0.01" unit="ms"/>
	</result>
</cmd>
