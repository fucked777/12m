<?xml version="1.0" encoding="utf-8"?>
<!--
id  枚举ID  -与设备类型关联
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums ExtenType="PAS" ModeType = "0xFFFF" edition="1.1.1.1">
	<enum name="SwitchingMode" desc="切换方式">
		<entry svalue="1" uvalue="1" desc="自动" />
		<entry svalue="2" uvalue="2" desc="手动" />
	</enum>
	<enum name="WorkingNumber" desc="工作机号">
		<entry svalue="1" uvalue="1" desc="A主B备" />
		<entry svalue="2" uvalue="2" desc="B主A备" />
	</enum>
	<enum name="RFSwitcher" desc="激励开关">
		<entry svalue="1" uvalue="1" desc="打开" />
		<entry svalue="2" uvalue="2" desc="关闭" />
	</enum>
	<enum name="PowerOutput" desc="输出功率去向_功率输出">
		<entry svalue="1" uvalue="1" desc="天线" />
		<entry svalue="2" uvalue="2" desc="负载" />
	</enum>
	<enum name="AmplifMode" desc="告警模式">
		<entry svalue="1" uvalue="1" desc="自动" />
		<entry svalue="2" uvalue="2" desc="手动" />
	</enum>
	<enum name="AmplifSwitcher" desc="功率告警开关">
		<entry svalue="1" uvalue="1" desc="开" />
		<entry svalue="2" uvalue="2" desc="关" />
	</enum>
	<enum name="CtrlMode" desc="控制模式">
		<entry svalue="1" uvalue="1" desc="分控" />
		<entry svalue="2" uvalue="2" desc="本控" />
	</enum>
</enums>
