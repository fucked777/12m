<?xml version="1.0" encoding="utf-8"?>
<!--
id  枚举ID  -与设备类型关联
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums ExtenType="TDFE" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ChanSendNumb" desc="发射通道号">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="通道1"/>
		<entry svalue="2" uvalue="2" desc="通道2"/>
	</enum>
	<enum name="ChannelType" desc="通道类型">
		<entry svalue="2" uvalue="2" desc="跟踪接收"/>
	</enum>
	<enum name="ChanReceNumb" desc="接收通道号">
		<entry svalue="1" uvalue="1" desc="S1接收"/>
		<entry svalue="3" uvalue="3" desc="S2接收"/>
		<!-- <entry svalue="5" uvalue="5" desc="通道5" /> -->
		<!--<entry svalue="2" uvalue="2" desc="通道2" />
		<entry svalue="3" uvalue="3" desc="通道3" />
		<entry svalue="4" uvalue="4" desc="通道4" />-->
	</enum>
	<enum name="ChanReceNumbs" desc="接收通道号">
		<entry svalue="1" uvalue="1" desc="S1接收"/>
		<!--<entry svalue="2" uvalue="2" desc="通道2" />-->
		<entry svalue="3" uvalue="3" desc="S2接收"/>
		<!--
		<entry svalue="5" uvalue="5" desc="通道5" />-->
		<entry svalue="5" uvalue="5" desc="L接收"/>
	</enum>
