		<field pid="10" id="PerigeeAngle" desc="ω近地点幅角" dataType="int" displayFormat="TracksNumber" initValue="" minValue="" maxValue="" step="" unit="°" formula="pow(2.0, 22)"/>
		<field pid="11" id="HorizontalAngle" desc="M平近点角" dataType="int" displayFormat="TracksNumber" initValue="" minValue="" maxValue="" step="" unit="°" formula="pow(2.0, 22)"/>
