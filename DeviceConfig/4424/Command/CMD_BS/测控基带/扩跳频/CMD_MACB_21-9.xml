<?xml version="1.0" encoding="UTF-8"?>
<!--
require:查询数据包 ,response:响应数据包,result:上报数据包
name:为字段名,对应相应设备参数xml文件中的参数名
fieldType:(code:命令码,data:数据,const:固定值)
dataType:数据类型(char,int8,uint8,short,ushort,int,uint,float,double,long,ulong,char|n)
displayFormat:界面显示类型(char,Int8,UInt8,Int16,UInt16,Int32,UInt32,float,double,Int64,UInt64,String,Bit,IP)
dataType中char|n:字符数组长度,如char|6
displayFormat中IP:按IP地址显示
displayFormat中Bit:按位进行显示
initValue为初始化值,
length为标识长度
ExtenType:分机类型
SelfLoopNumb:自身循环N次
recycle:循环  id表示循环次数的字段名  <recycle id="TargetNum"></recycle>  出现多个时,在名称前面加数字进行排序,如1TargetNum
cmdID:命令ID  与命令序号关联
isShow:0表示界面不显示  1表示界面显示
-->
<cmd cmdID="9" ExtenType="MACB" ModeType="0x21" desc="基带校零结果查询" flag="1001">
    <request MessageType="0x0000F001">
        <field pid="102" id="TaskIdent" desc="任务标识" dataType="uint" displayFormat="Hex" initValue="1" minValue="0" maxValue="FFFFFFFF" step="" unit=""/>
        <field pid="103" id="EquipComb" desc="设备组合号" dataType="ulong" displayFormat="Hex" initValue="" minValue="0" maxValue="FFFFFFFFFFFFFFFF" step="" unit=""/>
        <field pid="104" id="UplinkFreq" desc="上行频率" dataType="ulong" displayFormat="double" initValue="" minValue="2025" maxValue="2120" step="0.000001" unit="MHz" isShow="0"/>
        <field pid="105" id="DownFreq" desc="下行频率" dataType="ulong" displayFormat="double" initValue="" minValue="2200" maxValue="2300" step="0.000001" unit="MHz" isShow="0"/>
        <field pid="106" id="AdditIdent1" desc="附加标识1" dataType="uint" displayFormat="Hex" initValue="" minValue="" maxValue="" step="" unit="" isShow="0"/>
        <field pid="107" id="AdditIdent2" desc="附加标识2" dataType="uint" displayFormat="Hex" initValue="" minValue="" maxValue="" step="" unit="" isShow="0"/>
    </request>
    <result MessageType="0x0000F101">
        <field pid="2" id="TaskIdent" desc="任务标识" dataType="uint" displayFormat="Hex" initValue="" minValue="0" maxValue="FFFFFFFF" step="" unit=""/>
        <field pid="3" id="EquipComb" desc="设备组合号" dataType="ulong" displayFormat="Hex" initValue="" minValue="0" maxValue="FFFFFFFFFFFFFFFF" step="" unit=""/>
        <field pid="4" id="UplinkFreq" desc="上行频率" dataType="ulong" displayFormat="double" initValue="" minValue="2025" maxValue="2120" step="0.000001" unit="MHz" isShow="0"/>
        <field pid="5" id="DownFreq" desc="下行频率" dataType="ulong" displayFormat="double" initValue="" minValue="2200" maxValue="2300" step="0.000001" unit="MHz" isShow="0"/>
        <field pid="6" id="AdditIdent1" desc="附加标识1" dataType="uint" displayFormat="Hex" initValue="" minValue="" maxValue="" step="" unit="" isShow="0"/>
        <field pid="7" id="AdditIdent2" desc="附加标识2" dataType="uint" displayFormat="Hex" initValue="" minValue="" maxValue="" step="" unit="" isShow="0"/>
        <field pid="8" id="DistZeroMean" desc="距离零值均值" dataType="int" displayFormat="double" initValue="" minValue="" maxValue="" step="0.01" unit="m"/>
        <field pid="9" id="DistZeroVar" desc="距离零值方差" dataType="uint" displayFormat="double" initValue="" minValue="" maxValue="" step="0.001" unit="m"/>
    </result>
</cmd>
