<?xml version="1.0" encoding="utf-8"?>
<!--
require:查询数据包 ,response:响应数据包,result:上报数据包
name:为字段名,对应相应设备参数xml文件中的参数名
fieldType:(code:命令码,data:数据,const:固定值)
dataType:数据类型(char,int8,uint8,short,ushort,int,uint,float,double,long,ulong,char|n)
displayFormat:界面显示类型(char,Int8,UInt8,Int16,UInt16,Int32,UInt32,float,double,Int64,UInt64,String,Bit,IP)
dataType中char|n:字符数组长度,如char|6
displayFormat中IP:按IP地址显示
displayFormat中Bit:按位进行显示
initValue为初始化值,
length为标识长度
ExtenType:分机类型
SelfLoopNumb:自身循环N次
recycle:循环  id表示循环次数的字段名  <recycle id="TargetNum"></recycle>  出现多个时,在名称前面加数字进行排序,如1TargetNum
cmdID:命令ID  与命令序号关联
isShow:0表示界面不显示  1表示界面显示
-->
<cmd cmdID="10" ExtenType="MACB" ModeType="0x10" desc="等效环路带宽" flag="1001">
    <request MessageType="0x0000F001">
        <field pid="101" id="TestCtrl" desc="测试控制" dataType="uint8" displayFormat="enum|TestCtrl" initValue="" minValue="" maxValue="" step="" unit=""/>
        <field pid="102" id="Channel" desc="通道号" dataType="uint8" displayFormat="enum|Channels" initValue="" minValue="" maxValue="" step="" unit=""/>
        <field pid="103" id="RotatDirect" desc="旋向" dataType="uint8" displayFormat="enum|RotatDirect" initValue="1" minValue="" maxValue="" step="" unit="" isShow="0"/>
    </request>
    <result MessageType="0x0000F101">
        <field pid="1" id="TestStatus" desc="测试状态" dataType="uint8" displayFormat="enum|TestCtrlStatus" initValue="" minValue="" maxValue="" step="" unit=""/>
        <field pid="2" id="RotatDirect" desc="旋向" dataType="uint8" displayFormat="enum|RotatDirect" initValue="" minValue="" maxValue="" step="" unit="" isShow="0"/>
        <field pid="3" id="Bandwidth5" desc="带宽100Hz" dataType="ushort" displayFormat="UInt16" initValue="" minValue="" maxValue="" step="1" unit="Hz"/>
        <field pid="4" id="Bandwidth10" desc="带宽300Hz" dataType="ushort" displayFormat="UInt16" initValue="" minValue="" maxValue="" step="1" unit="Hz"/>
        <field pid="5" id="Bandwidth100" desc="带宽1000Hz" dataType="ushort" displayFormat="UInt16" initValue="" minValue="" maxValue="" step="1" unit="Hz"/>
        <field pid="6" id="Bandwidth500" desc="带宽3000Hz" dataType="ushort" displayFormat="UInt16" initValue="" minValue="" maxValue="" step="1" unit="Hz"/>
        <field pid="7" id="Bandwidth1000" desc="带宽1kHz" dataType="ushort" displayFormat="UInt16" initValue="" minValue="" maxValue="" step="1" unit="Hz" isShow="0"/>
        <field pid="8" id="Bandwidth2000" desc="带宽2kHz" dataType="ushort" displayFormat="UInt16" initValue="" minValue="" maxValue="" step="1" unit="Hz" isShow="0"/>
    </result>
</cmd>