<?xml version="1.0" encoding="utf-8"?>
<!--
id  枚举ID  -与设备类型关联
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums ExtenType="TB" ModeType = "0x01" edition="1.1.1.1">
	<enum name="ChanSelect" desc="通道选择">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="左旋" />
		<entry svalue="2" uvalue="2" desc="右旋" />
		<entry svalue="3" uvalue="3" desc="择优" />
	</enum>

	<enum name="PhaseCtrl" desc="校相控制">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="开始校相" />
		<!--<entry svalue="2" uvalue="2" desc="灵敏度调整" />-->
		<entry svalue="3" uvalue="3" desc="保存结果" />
		<entry svalue="4" uvalue="4" desc="停止校相" />
	</enum>

	<enum name="SignalRotat" desc="信号旋向">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="左旋" />
		<entry svalue="2" uvalue="2" desc="右旋" />
	</enum>

