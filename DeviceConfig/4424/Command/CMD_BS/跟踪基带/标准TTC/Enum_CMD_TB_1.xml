		<entry svalue="5" uvalue="5" desc="保存结果" />
	</enum>

	<enum name="FPhaseCtrl" desc="快速校相_校相控制">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="开始校相" />
		<entry svalue="2" uvalue="2" desc="停止校相" />
	</enum>

	<enum name="FPhaseStatus" desc="快速校相_校相状态">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="正在校相" />
		<entry svalue="2" uvalue="2" desc="校相完成" />
		<entry svalue="3" uvalue="3" desc="校相失败" />
	</enum>

