<?xml version="1.0" encoding="utf-8"?>
<!--
id  枚举ID  -与设备类型关联
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums ExtenType="TB" ModeType = "0x07" edition="1.1.1.1">
	<enum name="TargetNumb" desc="目标号">
		<entry svalue="1" uvalue="1" desc="一体化目标(固定为1)" />
		<entry svalue="2" uvalue="2" desc="扩频目标" />
	</enum>

	<enum name="TrackPol" desc="跟踪策略">
		<entry svalue="1" uvalue="1" desc="左旋" />
		<entry svalue="2" uvalue="2" desc="右旋" />
		<entry svalue="3" uvalue="3" desc="最大比合成" />
	</enum>

	<enum name="TrackTarget" desc="跟踪目标">
		<entry svalue="1" uvalue="1" desc="一体化目标" />
		<entry svalue="2" uvalue="2" desc="扩频目标" />
	</enum>

	<enum name="TrackChan" desc="跟踪通道">
		<entry svalue="1" uvalue="1" desc="遥测" />
		<entry svalue="2" uvalue="2" desc="测量" />
	</enum>

	<enum name="ChanSelect" desc="通道选择">
		<entry svalue="1" uvalue="1" desc="一体化目标" />
		<entry svalue="2" uvalue="2" desc="扩频目标" />
	</enum>

	<enum name="PhaseCtrl" desc="校相控制">
		<entry svalue="1" uvalue="1" desc="开始校相" />
		<!--<entry svalue="2" uvalue="2" desc="灵敏度调整" />-->
		<entry svalue="3" uvalue="3" desc="保存结果" />
		<entry svalue="4" uvalue="4" desc="停止校相" />
	</enum>

	<enum name="SignalRotat" desc="信号旋向">
		<entry svalue="1" uvalue="1" desc="左旋" />
		<entry svalue="2" uvalue="2" desc="右旋" />
	</enum>

	<enum name="PhaseStatus" desc="校相状态">
		<entry svalue="1" uvalue="1" desc="正在校相" />
		<entry svalue="2" uvalue="2" desc="校相完成" />
		<entry svalue="3" uvalue="3" desc="校相失败" />
		<!--<entry svalue="4" uvalue="4" desc="灵敏度调整" />-->
		<entry svalue="5" uvalue="5" desc="保存结果" />
	</enum>

	<enum name="FPhaseCtrl" desc="快速校相_校相控制">
		<entry svalue="1" uvalue="1" desc="开始校相" />
		<entry svalue="2" uvalue="2" desc="停止校相" />
	</enum>

	<enum name="FPhaseStatus" desc="快速校相_校相状态">
		<entry svalue="1" uvalue="1" desc="正在校相" />
		<entry svalue="2" uvalue="2" desc="校相完成" />
		<entry svalue="3" uvalue="3" desc="校相失败" />
	</enum>

	<enum name="AnteOffset" desc="天线偏移方式">
		<entry svalue="1" uvalue="1" desc="方位偏移" />
		<entry svalue="2" uvalue="2" desc="俯仰偏移" />
	</enum>

	<enum name="CarrOutput" desc="载波输出">
		<entry svalue="1" uvalue="1" desc="输出" />
		<entry svalue="2" uvalue="2" desc="停止输出" />
	</enum>

	<enum name="OutChanSelect" desc="上行载波输出_通道选择">
		<entry svalue="1" uvalue="1" desc="遥控通道1" />
		<entry svalue="2" uvalue="2" desc="遥控通道2" />
		<entry svalue="3" uvalue="3" desc="遥控通道3" />
		<entry svalue="4" uvalue="4" desc="测量通道1" />
		<entry svalue="5" uvalue="5" desc="测量通道2" />
		<entry svalue="6" uvalue="6" desc="测量通道3" />
		<entry svalue="7" uvalue="7" desc="全部通道" />
		<entry svalue="8" uvalue="8" desc="全部遥控通道" />
		<entry svalue="9" uvalue="9" desc="全部测量通道" />
	</enum>

	<enum name="CarrieOut" desc="载波输出">
		<entry svalue="1" uvalue="1" desc="输出" />
		<entry svalue="2" uvalue="2" desc="停止输出" />
	</enum>

	<enum name="WorkMode" desc="工作模式">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="标准TTC" />
		<entry svalue="2" uvalue="2" desc="扩频TTC" />
		<entry svalue="3" uvalue="3" desc="一体化跟踪" />
		<entry svalue="4" uvalue="4" desc="数传跟踪" />
		<entry svalue="5" uvalue="5" desc="标准TTC+数传跟踪" />
		<entry svalue="6" uvalue="6" desc="扩频TTC+数传跟踪" />
		<entry svalue="7" uvalue="7" desc="一体化+数传跟踪" />
		<entry svalue="8" uvalue="8" desc="一体化+扩频跟踪" />
		<entry svalue="9" uvalue="9" desc="扩跳频跟踪" />
	</enum>

	<enum name="WorkWay" desc="工作方式">
		<entry svalue="1" uvalue="1" desc="任务方式"/>
		<entry svalue="3" uvalue="3" desc="数传数字化闭环"/>
		<entry svalue="5" uvalue="5" desc="数传中频闭环"/>
	</enum>
	<enum name="OnlineSelect" desc="主备选择">
		<entry svalue="1" uvalue="1" desc="主机" />
		<entry svalue="2" uvalue="2" desc="备机" />
	</enum>
	<enum name="TimeSelect" desc="时统选择">
		<entry svalue="1" uvalue="1" desc="外时统"/>
		<entry svalue="2" uvalue="2" desc="内时统"/>
	</enum>
</enums>
