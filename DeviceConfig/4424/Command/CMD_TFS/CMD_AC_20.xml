<?xml version="1.0" encoding="UTF-8"?>
<!--
require:查询数据包 ,response:响应数据包,result:上报数据包
name:为字段名,对应相应设备参数xml文件中的参数名
fieldType:(code:命令码,data:数据,const:固定值)
dataType:数据类型(char,int8,uint8,short,ushort,int,uint,float,double,long,ulong,char|n)
displayFormat:界面显示类型(char,Int8,UInt8,Int16,UInt16,Int32,UInt32,float,double,Int64,UInt64,String,Bit,IP)
dataType中char|n:字符数组长度,如char|6
displayFormat中IP:按IP地址显示
displayFormat中Bit:按位进行显示
initValue为初始化值,
length为标识长度
ExtenType:分机类型
SelfLoopNumb:自身循环N次
recycle:循环  id表示循环次数的字段名  <recycle id="TargetNum"></recycle>  出现多个时,在名称前面加数字进行排序,如1TargetNum
cmdID:命令ID  与命令序号关联
isShow:0表示界面不显示  1表示界面显示
-->
<cmd cmdID="20" ExtenType="AC" ModeType="0xFFFF" desc="AGC曲线选择" flag="1001" isShow="0">
    <request MessageType="0x0000F001">
		<field pid="101" id="TaskIdent" desc="任务标识" dataType="uint" displayFormat="Hex" initValue="" minValue="" maxValue="FFFFFFFF" step="" unit=""/>
        <field pid="102" id="EquipNumb" desc="设备组合号" dataType="ulong" displayFormat="Hex" initValue="" minValue="" maxValue="FFFFFFFFFFFFFFFF" step="" unit=""/>
        <field pid="103" id="DownRadFreq" desc="下行射频频率" dataType="ulong" displayFormat="UInt64" initValue="" minValue="" maxValue="" step="" unit="Hz"/>
        <field pid="104" id="ReceSelect" desc="接收机选择" dataType="uint8" displayFormat="enum|ReceSelect" initValue="" minValue="" maxValue="" step="" unit=""/>
        <field pid="105" id="ReceSideLobe" desc="SΦ门限(主)" dataType="ushort" displayFormat="double" initValue="" minValue="" maxValue="" step="0.1" unit="dB"/>
        <field pid="106" id="DiffReceSide" desc="SΦ门限(主-引导)" dataType="ushort" displayFormat="double" initValue="" minValue="" maxValue="" step="0.1" unit="dB"/>
    </request>
    <result MessageType="0x0000F101">
        <field pid="1" id="TaskIdent" desc="选择结果" dataType="uint8" displayFormat="enum|SelectResult" initValue="" minValue="" maxValue="" step="" unit=""/>
    </result>
</cmd>
