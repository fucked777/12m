<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="SXFC" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlWay" desc="监控方式">
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="DeviceSwitch" desc="设备开关电">
		<entry svalue="1" uvalue="1" desc="关电:R"/>
		<entry svalue="2" uvalue="2" desc="开电:G"/>
	</enum>
	<enum name="ClockModeStatus" desc="时钟模块状态">
		<entry svalue="1" uvalue="1" desc="锁定:G"/>
		<entry svalue="2" uvalue="2" desc="失锁:R"/>
	</enum>
	<enum name="LocalFlapModeStatus" desc="本振模块状态">
		<entry svalue="1" uvalue="1" desc="锁定:G"/>
		<entry svalue="2" uvalue="2" desc="失锁:R"/>
	</enum>
	<enum name="BeconModeStatus" desc="信标模块状态">
		<entry svalue="1" uvalue="1" desc="锁定:G"/>
		<entry svalue="2" uvalue="2" desc="失锁:R"/>
	</enum>
	<enum name="XLFCChannelElectricStatus" desc="校零变频器通道电流状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	<enum name="LocalFlapModeElectricStatus" desc="本振模块电流状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	<enum name="BeconModeElectricStatus" desc="信标模块电流状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	<enum name="DeviceStatus" desc="设备状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	<enum name="XLBeconSwitch" desc="校零信标选择开关">
		<entry svalue="1" uvalue="1" desc="校零输出"/>
		<entry svalue="2" uvalue="2" desc="信标输出"/>
		<entry svalue="3" uvalue="3" desc="校零信标都关"/>
	</enum>
	<enum name="10MRefer" desc="10M参考源">
		<entry svalue="1" uvalue="1" desc="内参考"/>
		<entry svalue="2" uvalue="2" desc="外参考"/>
	</enum>
</enums>
