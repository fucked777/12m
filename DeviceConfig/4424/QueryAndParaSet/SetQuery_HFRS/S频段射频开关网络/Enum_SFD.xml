<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="SFD" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="DeviceSwitch" desc="设备开关电">
		<entry svalue="1" uvalue="1" desc="关电:R"/>
		<entry svalue="2" uvalue="2" desc="开电:G"/>
	</enum>
	<enum name="Switch" desc="开关">
		<!--<entry svalue="1" uvalue="1" desc="接点1->+28V,接点2->0V"/>
		<entry svalue="2" uvalue="2" desc="接点1->0V,接点2->+28V"/>-->
		<entry svalue="1" uvalue="1" desc="左旋"/>
		<entry svalue="2" uvalue="2" desc="右旋"/>
	</enum>
	<enum name="DiffSwitch" desc="开关">
		<entry svalue="1" uvalue="1" desc="左差选S-LNA5,右差选S-LNA6"/>
		<entry svalue="2" uvalue="2" desc="左差选S-LNA6,右差选S-LNA5"/>
	</enum>
	<enum name="LSumSwitch" desc="开关">
		<entry svalue="1" uvalue="1" desc="S-LNA1"/>
		<entry svalue="2" uvalue="2" desc="S-LNA2"/>
	</enum>
	<enum name="RSumSwitch" desc="开关">
		<entry svalue="1" uvalue="1" desc="S-LNA3"/>
		<entry svalue="2" uvalue="2" desc="S-LNA4"/>
	</enum>
	<enum name="SLNAElectricStatus" desc="S-LNA电流状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	<enum name="SLNAElectric" desc="S-LNA加去电">
		<entry svalue="1" uvalue="1" desc="加电"/>
		<entry svalue="2" uvalue="2" desc="去电"/>
	</enum>
</enums>
