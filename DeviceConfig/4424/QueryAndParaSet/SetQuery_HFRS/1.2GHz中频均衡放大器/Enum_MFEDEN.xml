<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="MFEDEN" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlWay" desc="监控方式">
			<entry svalue="1" uvalue="1" desc="分控" />
			<entry svalue="2" uvalue="2" desc="本控" />
        </enum>
		<enum name="DeviceSwitch" desc="设备开关电">
			<entry svalue="1" uvalue="1" desc="关电:R" />
			<entry svalue="2" uvalue="2" desc="开电:G" />
        </enum>
		<enum name="ModeStatus" desc="模块状态">
			<entry svalue="1" uvalue="1" desc="正常:G" />
			<entry svalue="2" uvalue="2" desc="异常:R" />
        </enum>

		<enum name="PowerStatus" desc="电流状态">
			<entry svalue="1" uvalue="1" desc="正常:G" />
			<entry svalue="2" uvalue="2" desc="异常:R" />
        </enum>
		<enum name="DeviceStatus" desc="设备状态">
			<entry svalue="1" uvalue="1" desc="正常:G" />
			<entry svalue="2" uvalue="2" desc="异常:R" />
        </enum>
		<enum name="KaSwitchS1" desc="Ka开关S1">
			<entry svalue="1" uvalue="1" desc="左旋输入" />
			<entry svalue="2" uvalue="2" desc="备份输入" />
        </enum>
		<enum name="KaSwitchS2" desc="Ka开关S2">
			<entry svalue="1" uvalue="1" desc="右旋输入" />
			<entry svalue="2" uvalue="2" desc="备份输入" />
        </enum>
</enums>
