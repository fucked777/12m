		<field pid="5" GroupId="3" id="ControlMode5" desc="发射光状态" dataType="uint8" displayFormat="LED|ControStatus" initValue="" minValue="" maxValue="" step="" unit=""/>
		<field pid="6" GroupId="3" id="ControlMode6" desc="发射光功率值" dataType="uint8" displayFormat="UInt8" initValue="" minValue="0" maxValue="14" step="" unit="dBm"/>
