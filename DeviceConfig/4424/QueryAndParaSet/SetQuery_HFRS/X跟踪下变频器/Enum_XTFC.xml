<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="XTFC" ModeType="0xFFFF" edition="1.1.1.1">
		<enum name="ControlWay" desc="监控方式">
			<entry svalue="1" uvalue="1" desc="分控" />
			<entry svalue="2" uvalue="2" desc="本控" />
        </enum>
		<enum name="DeviceSwitch" desc="设备开关电">
			<entry svalue="1" uvalue="1" desc="关电:R" />
			<entry svalue="2" uvalue="2" desc="开电:G" />
        </enum>
		<enum name="ClockLockStatus" desc="时钟锁定">
			<entry svalue="1" uvalue="1" desc="锁定:G" />
			<entry svalue="2" uvalue="2" desc="失锁:R" />
        </enum>
		<enum name="PowerModeStatus" desc="电源模块状态">
			<entry svalue="1" uvalue="1" desc="正常:G" />
			<entry svalue="2" uvalue="2" desc="异常:R" />
        </enum>
		<enum name="BeconModeStatus" desc="本振锁定">
			<entry svalue="1" uvalue="1" desc="锁定:G" />
			<entry svalue="2" uvalue="2" desc="失锁:R" />
        </enum>
		<enum name="ElectricStatus" desc="电流状态">
			<entry svalue="1" uvalue="1" desc="正常:G" />
			<entry svalue="2" uvalue="2" desc="异常:R" />
        </enum>
		<enum name="DeviceStatus" desc="设备状态">
			<entry svalue="1" uvalue="1" desc="正常:G" />
			<entry svalue="2" uvalue="2" desc="异常:R" />
        </enum>
		<enum name="InOutReference" desc="内外参考">
			<entry svalue="1" uvalue="1" desc="内参考" />
			<entry svalue="2" uvalue="2" desc="外参考" />
        </enum>
</enums>
