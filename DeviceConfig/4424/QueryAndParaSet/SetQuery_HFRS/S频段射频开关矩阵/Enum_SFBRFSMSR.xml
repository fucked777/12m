<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="SFBRFSMSR" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="DeviceSwitch" desc="设备开关电">
		<entry svalue="1" uvalue="1" desc="故障:R"/>
		<entry svalue="2" uvalue="2" desc="正常:G"/>
	</enum>
	<enum name="PortStatus" desc="端口状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	<enum name="PowerStatus" desc="电源状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	<enum name="DeviceStatus" desc="设备状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	<enum name="InputPort" desc="输入端口">
		<entry svalue="0" uvalue="0" desc="关"/>
		<entry svalue="1" uvalue="1" desc="S左旋和路"/>
		<entry svalue="2" uvalue="2" desc="S右旋和路"/>
		<entry svalue="3" uvalue="3" desc="跟踪左旋差路"/>
		<entry svalue="4" uvalue="4" desc="跟踪右旋差路"/>
		<entry svalue="5" uvalue="5" desc="小环监测"/>
		<entry svalue="6" uvalue="6" desc="射频模拟源1"/>
		<entry svalue="7" uvalue="7" desc="射频模拟源2"/>
		<entry svalue="8" uvalue="8" desc="常规联试应答机信号"/>
		<entry svalue="9" uvalue="9" desc="扩跳联试应答机信号"/>
		<entry svalue="10" uvalue="10" desc="射频前端监测信号"/>
		<entry svalue="11" uvalue="11" desc="射频输入备用1"/>
		<entry svalue="12" uvalue="12" desc="射频输入备用2"/>
	</enum>
	<enum name="RealInputPort" desc="输入端口">
		<entry svalue="0" uvalue="0" desc="关"/>
		<entry svalue="1" uvalue="1" desc="左旋和路"/>
		<entry svalue="2" uvalue="2" desc="右旋差路"/>
		<entry svalue="3" uvalue="3" desc="跟踪和路"/>
		<entry svalue="4" uvalue="4" desc="跟踪差路"/>
	</enum>
</enums>
