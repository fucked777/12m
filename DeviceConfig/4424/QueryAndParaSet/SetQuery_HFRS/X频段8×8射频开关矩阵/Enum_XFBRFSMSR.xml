<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="XFBRFSMSR" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="1" uvalue="1" desc="分控" />
		<entry svalue="2" uvalue="2" desc="本控" />
	</enum>
	<enum name="DeviceSwitch" desc="设备开关电">
		<entry svalue="1" uvalue="1" desc="故障:R" />
		<entry svalue="2" uvalue="2" desc="正常:G" />
	</enum>
	
	<enum name="PortStatus" desc="端口状态">
		<entry svalue="1" uvalue="1" desc="正常:G" />
		<entry svalue="2" uvalue="2" desc="异常:R" />
	</enum>
	
	<enum name="PowerStatus" desc="电源状态">
		<entry svalue="1" uvalue="1" desc="正常:G" />
		<entry svalue="2" uvalue="2" desc="异常:R" />
	</enum>
	
	<enum name="DeviceStatus" desc="设备状态">
		<entry svalue="1" uvalue="1" desc="正常:G" />
		<entry svalue="2" uvalue="2" desc="异常:R" />
	</enum>

	<enum name="InputPort" desc="输出端口">
		<entry svalue="0" uvalue="0" desc="关" />
		<entry svalue="1" uvalue="1" desc="X下行左" />
		<entry svalue="2" uvalue="2" desc="X下行右" />
		<entry svalue="3" uvalue="3" desc="X下行备" />
		<!--
		<entry svalue="4" uvalue="4" desc="4(备用)" />
		<entry svalue="5" uvalue="5" desc="5(备用)" />
		<entry svalue="6" uvalue="6" desc="6(备用)" />
		<entry svalue="7" uvalue="7" desc="7(备用)" />
		<entry svalue="8" uvalue="8" desc="8(备用)" />
		-->
	</enum>

</enums>
