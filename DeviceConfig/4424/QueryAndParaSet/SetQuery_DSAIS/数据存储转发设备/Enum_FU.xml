<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="FU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="WorkStatus" desc="工作状态">
		<entry svalue="1" uvalue="1" desc="空闲"/>
		<entry svalue="2" uvalue="2" desc="实时原速转发"/>
		<entry svalue="3" uvalue="3" desc="实时降速转发"/>
		<entry svalue="4" uvalue="4" desc="事后转发"/>
		<entry svalue="5" uvalue="5" desc="转发错误"/>
	</enum>
	<enum name="CenterSendStatus" desc="中心发送状态">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="正常发送"/>
		<entry svalue="2" uvalue="2" desc="文件操作错误"/>
		<entry svalue="3" uvalue="3" desc="网络通信异常"/>
		<entry svalue="4" uvalue="4" desc="其他原因"/>
	</enum>
	<enum name="TCPProtocol" desc="传输层协议">
		<entry svalue="1" uvalue="1" desc="PDXP"/>
		<entry svalue="2" uvalue="2" desc="FEP"/>
	</enum>
</enums>
