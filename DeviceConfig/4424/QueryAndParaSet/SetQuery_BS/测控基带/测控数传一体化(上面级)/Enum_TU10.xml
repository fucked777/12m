		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="无填充"/>
		<entry svalue="2" uvalue="2" desc="有填充"/>
	</enum>
	<enum name="CodeType" desc="码型">
		<entry svalue="1" uvalue="1" desc="NRZ-L"/>
