<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="TTCLHR" ModeType="0x01" edition="1.1.1.1">
    <enum name="CarrierLockInd" desc="载波锁定">
        <entry svalue="0" uvalue="0" desc="无效"/>
        <entry svalue="1" uvalue="1" desc="锁定:G"/>
        <entry svalue="2" uvalue="2" desc="失锁:R"/>
    </enum>
    <enum name="FollowUpInst" desc="随扫指示">
        <entry svalue="0" uvalue="0" desc="无此参数"/>
        <entry svalue="1" uvalue="1" desc="未随扫:Y"/>
        <entry svalue="2" uvalue="2" desc="随扫:G"/>
    </enum>
    <enum name="ReceivingChannel" desc="接收通道">
        <entry svalue="0" uvalue="0" desc="无此参数"/>
        <entry svalue="1" uvalue="1" desc="左旋"/>
        <entry svalue="2" uvalue="2" desc="右旋"/>
        <!--<entry svalue="3" uvalue="3" desc="同时(遥测有效)"/>-->
    </enum>
    <enum name="AGCTimeConstant" desc="AGC时间常数">
        <entry svalue="0" uvalue="0" desc="无此参数"/>
        <entry svalue="1" uvalue="1" desc="1ms"/>
        <entry svalue="2" uvalue="2" desc="10ms"/>
        <entry svalue="3" uvalue="3" desc="100ms"/>
        <entry svalue="4" uvalue="4" desc="1000ms"/>
    </enum>
    <enum name="CarrierLoopBandWidth" desc="载波环路带宽">
        <entry svalue="0" uvalue="0" desc="无此参数"/>
        <entry svalue="1" uvalue="1" desc="5Hz"/>
        <entry svalue="2" uvalue="2" desc="10Hz"/>
        <entry svalue="3" uvalue="3" desc="100Hz"/>
        <entry svalue="4" uvalue="4" desc="500Hz"/>
        <entry svalue="5" uvalue="5" desc="1000Hz"/>
        <entry svalue="6" uvalue="6" desc="2000Hz"/>
    </enum>
</enums>