<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="IFCM" ModeType="0x13" edition="1.1.1.1">
    <enum name="TotalCarrCtrl" desc="载波输出控制">
        <entry svalue="1" uvalue="1" desc="允许输出"/>
        <entry svalue="2" uvalue="2" desc="禁止输出"/>
    </enum>
    <enum name="CarrierModu" desc="调制方式">
        <entry svalue="0" uvalue="0" desc="无此参数"/>
        <entry svalue="1" uvalue="1" desc="去调"/>
        <entry svalue="2" uvalue="2" desc="加调"/>
    </enum>
    <enum name="NoiseOupput" desc="噪声源">
        <entry svalue="0" uvalue="0" desc="无此参数"/>
        <entry svalue="1" uvalue="1" desc="开"/>
        <entry svalue="2" uvalue="2" desc="关"/>
    </enum>
    <enum name="ChanCarrCtrl" desc="通道载波输出控制">
        <entry svalue="0" uvalue="0" desc="无此参数"/>
        <entry svalue="1" uvalue="1" desc="开"/>
        <entry svalue="2" uvalue="2" desc="关"/>
    </enum>
    <enum name="CarrierToZero" desc="载波回零">
        <entry svalue="1" uvalue="1" desc="未回零:Y"/>
        <entry svalue="2" uvalue="2" desc="回零:G"/>
    </enum>
    <enum name="ChannelCtrl" desc="通道输出控制">
        <entry svalue="0" uvalue="0" desc="无此参数"/>
        <entry svalue="1" uvalue="1" desc="允许"/>
        <entry svalue="2" uvalue="2" desc="禁止"/>
    </enum>
    <enum name="PNCodeAdd" desc="伪码加调">
        <entry svalue="0" uvalue="0" desc="无此参数"/>
        <entry svalue="1" uvalue="1" desc="加调"/>
        <entry svalue="2" uvalue="2" desc="去调"/>
    </enum>
    <enum name="DataTuning" desc="数据加调">
        <entry svalue="0" uvalue="0" desc="无此参数"/>
        <entry svalue="1" uvalue="1" desc="加调"/>
        <entry svalue="2" uvalue="2" desc="去调"/>
    </enum>
    <enum name="SpreadCode" desc="扩频码速率">
        <entry svalue="0" uvalue="0" desc="无此参数"/>
        <entry svalue="1" uvalue="1" desc="1*1.023Mcps"/>
        <entry svalue="2" uvalue="2" desc="2*1.023Mcps"/>
        <entry svalue="3" uvalue="3" desc="3*1.023Mcps"/>
        <entry svalue="4" uvalue="4" desc="4*1.023Mcps"/>
        <entry svalue="5" uvalue="5" desc="5*1.023Mcps"/>
        <entry svalue="6" uvalue="6" desc="6*1.023Mcps"/>
        <entry svalue="7" uvalue="7" desc="7*1.023Mcps"/>
        <entry svalue="8" uvalue="8" desc="8*1.023Mcps"/>
        <entry svalue="9" uvalue="9" desc="9*1.023Mcps"/>
        <entry svalue="10" uvalue="10" desc="10*1.023Mcps"/>
    </enum>
</enums>