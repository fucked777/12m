<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
set: 参数设置 ,result:状态结果上报
name:为字段名,对应相应设备参数xml文件中的参数名
fieldType:(code:命令码,data:数据,const:固定值)
dataType:数据类型(char,int8,uint8,short,ushort,int,uint,float,double,long,ulong,char|n)
displayFormat:界面显示类型(char,Int8,UInt8,Int16,UInt16,Int32,UInt32,float,double,Int64,UInt64,String,Bit,IP)
dataType中char|n:字符数组长度,如char|6
displayFormat中IP:按IP地址显示
displayFormat中Bit:按位进行显示
UnitType:单元类型
ModeType:模式类型
initValue:初始化值
recycle:循环  id表示循环次数的字段名  <recycle id="TargetNum"></recycle>  出现多个时,在名称前面加数字进行排序,如1TargetNum
isShow:0表示界面不显示  1表示界面显示
isControl:0表示状态 1表示设置
isAlarm:是否告警 0表示不告警 1表示告警
BitDivision:位取值BitDivision="1|2|3|5";如uint32为例,每1位都表示一个内容BitDivision="1";每2位都表示一个内容BitDivision="2";
每一个都不同时则用|分开,如BitDivision="1|2|3|5",1表示第一个为1位,2表示第二个紧跟着的2-3位的值,3表示第二个紧跟着的4-6位的值,5后面不写全表示5后面的字段保留;
-->


<cmd UnitType="SK2TBPU" ModeType="0x06" desc="公共单元" flag="1001" kpValid="NumbSpreadTarget">

	<result MessageType="0x0000F104">
		<field pid="1"  GroupId="status1" id="ControlMode" desc="监控方式" dataType="uint8" displayFormat="enum|ControlMode" initValue="" minValue="" maxValue="" step="" unit="" />
		<field pid="2"  GroupId="status1" id="BaseEquipInteStatus" desc="综合状态" dataType="uint8" displayFormat="LED|BaseEquipInteStatus" initValue="" minValue="" maxValue="" step="" unit="" />
		<field pid="3"  GroupId="status1" id="SbandOnline" desc="S频段在线" dataType="uint8" displayFormat="enum|bandOnline" initValue="" minValue="" maxValue="" step="" unit="" />
		<field pid="4"  GroupId="status1" id="KaXbandOnline" desc="Ka/X频段在线" dataType="uint8" displayFormat="enum|bandOnline" initValue="" minValue="" maxValue="" step="" unit="" />
		<field pid="5"  GroupId="status1" id="ExternalState" desc="外时码状态" dataType="uint8" displayFormat="LED|ExternalState" initValue="" minValue="" maxValue="" step="" unit="" />
		<field pid="6"  GroupId="status1" id="ExternalFreqState" desc="外频标状态" dataType="uint8" displayFormat="LED|ExternalFreqState" initValue="" minValue="" maxValue="" step="" unit="" />
		<field pid="7"  GroupId="status1" id="WorkMode" desc="工作模式" dataType="uint8" displayFormat="enum|WorkMode" initValue="" minValue="" maxValue="" step="" unit="" />
		<field pid="8"  GroupId="status1" id="WorkType" desc="工作方式" dataType="uint8" displayFormat="enum|WorkType" initValue="" minValue="" maxValue="" step="" unit="" />

		
		<field pid="101"  GroupId="status2" id="XKaTaskIdentifier" desc="X/Ka任务标识" dataType="uint" displayFormat="Hex" initValue="1" minValue="" maxValue="FFFFFFFF" step="" unit="" />
		<field pid="102"  GroupId="status2" id="XKaTaskCode" desc="X/Ka任务代号" dataType="char|6" displayFormat="String" initValue="1" minValue="0" maxValue="" step="" unit=""  />
		<field pid="103"  GroupId="status2" id="XKAEquipCombNum" desc="X/Ka设备组合号" dataType="ulong" displayFormat="Hex" initValue="1" minValue="0" maxValue="FFFFFFFFFFFFFFFF" step="" unit=""  />
		<field pid="104"  GroupId="status2" id="XKaDownFreq" desc="X/Ka下行频率" dataType="ulong" displayFormat="double" initValue="25000" minValue="7750|25000" maxValue="9000|27500" step="0.000001" unit="MHz"  />
		<field pid="105"  GroupId="status2" id="NumbSpreadTarget" desc="扩频目标数" dataType="uint8" displayFormat="UInt8" initValue="4" minValue="1" maxValue="4" step="" unit=""  />
		<field pid="106"  GroupId="status2" id="SpreadMastTarget" desc="扩频主跟目标" dataType="uint8" displayFormat="UInt8" initValue="4" minValue="1" maxValue="4" step="" unit=""  />
		<field pid="107"  GroupId="status2" id="SpreadChannel" desc="扩频主跟通道" dataType="uint8" displayFormat="enum|SpreadChannel" initValue="1" minValue="" maxValue="" step="" unit=""  />

		<!-- 新增参数/-->
		<field pid="108"  GroupId="status2" id="TrackRoatDirect" desc="扩频跟踪旋向" dataType="uint8" displayFormat="enum|TrackRoatDirect" initValue="1" minValue="" maxValue="" step="" unit=""  />
		<field pid="109"  GroupId="status2" id="SCTrackRoatDirect" desc="数传跟踪旋向" dataType="uint8" displayFormat="enum|TrackRoatDirect" initValue="1" minValue="" maxValue="" step="" unit=""  />
		<recycle id="1" recycleNum="4" desc="">
			<field pid="109" GroupId="status3" id="STaskIdentifier" desc="S任务标识" dataType="uint" displayFormat="Hex" initValue="1" minValue="" maxValue="FFFFFFFF" step="" unit=""  />
			<field pid="110" GroupId="status3" id="STaskCode" desc="S任务代号" dataType="char|6" displayFormat="String" initValue="1" minValue="0" maxValue="" step="" unit=""  />
			<field pid="111" GroupId="status3" id="SEquipCombNum" desc="S设备组合号" dataType="ulong" displayFormat="Hex" initValue="1" minValue="0" maxValue="FFFFFFFFFFFFFFFF" step="" unit=""  />
			<field pid="112" GroupId="status3" id="NumberSimul" desc="S下行频率" dataType="ulong" displayFormat="double" initValue="2200" minValue="2200" maxValue="2300" step="0.000001" unit="MHz"  />
		</recycle>
                    
	</result>
	
	<layout>
		<item id="status1" desc="公共参数" row="0" col = "1" rowspan = "1" colspan = "1"/>
		<item id="status2" desc="公共参数" row="0" col = "1" rowspan = "1" colspan = "1"/>
		<item id="status3" desc="" row="0" col = "1" rowspan = "1" colspan = "1"/>
	</layout>


	<order info="status1,status2,status3"/>
</cmd>
