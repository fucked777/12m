<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="TBPU" ModeType="0x07" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="0" uvalue="0" desc="无效" />
		<entry svalue="1" uvalue="1" desc="分控" />
		<entry svalue="2" uvalue="2" desc="本控" />
	</enum>

	<enum name="BaseEquipInteStatus" desc="设备综合状态">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="故障:R" />
		<entry svalue="2" uvalue="2" desc="正常:G" />
	</enum>

	<enum name="bandOnline" desc="频段在线状态">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="主机" />
		<entry svalue="2" uvalue="2" desc="备机" />
	</enum>

	<enum name="ExternalState" desc="外时码状态">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="故障:R" />
		<entry svalue="2" uvalue="2" desc="正常:G" />
	</enum>

	<enum name="ExternalFreqState" desc="外频标状态">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="故障:R" />
		<entry svalue="2" uvalue="2" desc="正常:G" />
	</enum>

	<enum name="WorkMode" desc="工作模式">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="标准TTC" />
		<entry svalue="2" uvalue="2" desc="扩频TTC" />
		<entry svalue="3" uvalue="3" desc="一体化跟踪" />
		<entry svalue="4" uvalue="4" desc="数传跟踪" />
		<entry svalue="5" uvalue="5" desc="标准TTC+数传跟踪" />
		<entry svalue="6" uvalue="6" desc="扩频TTC+数传跟踪" />
		<entry svalue="7" uvalue="7" desc="一体化+数传跟踪" />
		<entry svalue="8" uvalue="8" desc="一体化+扩频跟踪" />
		<entry svalue="9" uvalue="9" desc="扩跳频跟踪" />
	</enum>

	<enum name="SpreadChannel" desc="扩频主跟通道">
		<entry svalue="0" uvalue="0" desc="无此参数" />
		<entry svalue="1" uvalue="1" desc="遥测" />
		<entry svalue="2" uvalue="2" desc="测量" />
	</enum>

	<enum name="SpecDirecOutput" desc="指定输出旋向">
                	<entry svalue="0" uvalue="0" desc="无此参数" />
                	<entry svalue="1" uvalue="1" desc="左旋" />
                	<entry svalue="2" uvalue="2" desc="右旋" />
	</enum> 

	<enum name="WorkType" desc="工作方式">
		<entry svalue="1" uvalue="1" desc="任务方式"/>
		<entry svalue="3" uvalue="3" desc="数传数字化闭环"/>
		<entry svalue="5" uvalue="5" desc="数传中频闭环"/>
    </enum>
</enums>
