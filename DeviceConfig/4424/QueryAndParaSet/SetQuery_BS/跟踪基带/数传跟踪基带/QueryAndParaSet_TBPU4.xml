	</result>
		<layout>
			<item id="setting0" desc="公共参数" row="0" col = "0" rowspan = "1" colspan = "2"/>
		
			<item id="setting2" desc="高速数传参数" row="1" col = "0" rowspan = "1" colspan = "2"/>
