<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="KLRFSN" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="2" desc="设备开关电">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="关电:R"/>
		<entry svalue="2" uvalue="2" desc="开电:G"/>
	</enum>
	<enum name="3" desc="状态">
		<entry svalue="0" uvalue="0" desc="无此参数"/>
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	<enum name="NumBeforeChoose" desc="S上行出开关选择(X3)">
		<entry svalue="1" uvalue="1" desc="数字化前端A"/>
		<entry svalue="2" uvalue="2" desc="数字化前端B"/>
	</enum>
	<enum name="DownChangeFreqSwitchNet" desc="遥控小环选择(出)">
		<entry svalue="1" uvalue="1" desc="前端小环入"/>
		<entry svalue="2" uvalue="2" desc="功放小环入"/>
	</enum>
</enums>
