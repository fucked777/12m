<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="TTCRCH" ModeType="0x07" edition="1.1.1.1">
	<enum name="LockIndicat" desc="锁定指示">
		<entry svalue="1" uvalue="1" desc="锁定:G"/>
		<entry svalue="2" uvalue="2" desc="失锁:R"/>
	</enum>
	<enum name="WorkSwitch" desc="工作开关">
		<entry svalue="1" uvalue="1" desc="关"/>
		<entry svalue="2" uvalue="2" desc="开"/>
	</enum>
	<enum name="InputSelect" desc="输入选择">
		<entry svalue="1" uvalue="1" desc="左旋入"/>
		<!-- <entry svalue="2" uvalue="2" desc="右旋入"/> -->
	</enum>
	<enum name="CodingMode" desc="伪码开关">
		<entry svalue="1" uvalue="1" desc="关"/>
		<entry svalue="2" uvalue="2" desc="开"/>
	</enum>
	<enum name="CarrPNCode" desc="载波辅助伪码捕获">
		<!-- <entry svalue="1" uvalue="1" desc="无"/> -->
		<entry svalue="2" uvalue="2" desc="有"/>
	</enum>
	<enum name="CodeSpeed" desc="扩频码速率">
		<entry svalue="1" uvalue="1" desc="1*1.023Mcps"/>
		<entry svalue="2" uvalue="2" desc="2*1.023Mcps"/>
		<entry svalue="3" uvalue="3" desc="3*1.023Mcps"/>
		<entry svalue="4" uvalue="4" desc="4*1.023Mcps"/>
		<entry svalue="5" uvalue="5" desc="5*1.023Mcps"/>
		<entry svalue="6" uvalue="6" desc="6*1.023Mcps"/>
		<entry svalue="7" uvalue="7" desc="7*1.023Mcps"/>
		<entry svalue="8" uvalue="8" desc="8*1.023Mcps"/>
		<entry svalue="9" uvalue="9" desc="9*1.023Mcps"/>
		<entry svalue="10" uvalue="10" desc="10*1.023Mcps"/>
	</enum>
	<enum name="CodeType" desc="码型">
		<entry svalue="1" uvalue="1" desc="NRZ-L"/>
		<entry svalue="2" uvalue="2" desc="NRZ-M"/>
		<entry svalue="3" uvalue="3" desc="NRZ-S"/>
	</enum>
</enums>
