<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="PU" ModeType="0x02" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="CompEquipState" desc="联试应答机设备综合状态">
		<entry svalue="1" uvalue="1" desc="故障:R"/>
		<entry svalue="2" uvalue="2" desc="正常:G"/>
	</enum>
	<enum name="OnLineMachine" desc="本机在线状态">
		<entry svalue="1" uvalue="1" desc="主机"/>
		<entry svalue="2" uvalue="2" desc="备机"/>
	</enum>
	<enum name="ExternalState" desc="外时码状态">
		<entry svalue="1" uvalue="1" desc="故障:R"/>
		<entry svalue="2" uvalue="2" desc="正常:G"/>
	</enum>
	<enum name="ExternalFreqState" desc="外频标状态">
		<entry svalue="1" uvalue="1" desc="故障:R"/>
		<entry svalue="2" uvalue="2" desc="正常:G"/>
	</enum>
	<enum name="WorkingMode" desc="工作体制">
		<entry svalue="1" uvalue="1" desc="标准TTC"/>
		<entry svalue="2" uvalue="2" desc="扩频TTC"/>
		<entry svalue="3" uvalue="3" desc="一体化上面级"/>
		<entry svalue="4" uvalue="4" desc="一体化卫星"/>
		<entry svalue="5" uvalue="5" desc="一体化上面级+扩二共载波"/>
		<entry svalue="6" uvalue="6" desc="一体化上面级+扩二不共载波"/>
		<entry svalue="7" uvalue="7" desc="一体化卫星+扩二"/>
		<entry svalue="8" uvalue="8" desc="一体化上行高码率"/>
	</enum>
	<enum name="UpInChoose" desc="上行输入选择">
		<entry svalue="1" uvalue="1" desc="射频"/>
		<entry svalue="2" uvalue="2" desc="中频"/>
	</enum>
	<enum name="WorkWay" desc="工作方式">
		<entry svalue="1" uvalue="1" desc="动态仿真"/>
		<entry svalue="2" uvalue="2" desc="文件仿真"/>
	</enum>
	<enum name="BaseFeedMode" desc="基带送数方式">
		<entry svalue="1" uvalue="1" desc="常送"/>
		<entry svalue="2" uvalue="2" desc="人工"/>
	</enum>
	<enum name="CurrEmuStatus" desc="当前仿真状态">
		<entry svalue="1" uvalue="1" desc="停止仿真:Y"/>
		<entry svalue="2" uvalue="2" desc="正在仿真:G"/>
	</enum>
	<enum name="FileSimuLoop" desc="文件仿真循环回放标记">
		<entry svalue="1" uvalue="1" desc="否"/>
		<entry svalue="2" uvalue="2" desc="是"/>
	</enum>
</enums>
