		<entry svalue="4" uvalue="4" desc="4*1.023Mcps"/>
		<entry svalue="5" uvalue="5" desc="5*1.023Mcps"/>
		<entry svalue="6" uvalue="6" desc="6*1.023Mcps"/>
		<entry svalue="7" uvalue="7" desc="7*1.023Mcps"/>
		<entry svalue="8" uvalue="8" desc="8*1.023Mcps"/>
		<entry svalue="9" uvalue="9" desc="9*1.023Mcps"/>
		<entry svalue="10" uvalue="10" desc="10*1.023Mcps"/>
	</enum>
</enums>
