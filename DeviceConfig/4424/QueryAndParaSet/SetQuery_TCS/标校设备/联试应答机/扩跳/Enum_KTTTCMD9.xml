<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="KTTTCMD" ModeType="0x09" edition="1.1.1.1">
	<enum name="SendDataType" desc="当前上行发射数据类型">
		<entry svalue="1" uvalue="1" desc="时间"/>
		<entry svalue="2" uvalue="2" desc="遥控"/>
		<entry svalue="3" uvalue="3" desc="空闲"/>
		<entry svalue="4" uvalue="4" desc="其他"/>
	</enum>
	<enum name="CarrCtrl" desc="载波控制">
		<entry svalue="1" uvalue="1" desc="去载"/>
		<entry svalue="2" uvalue="2" desc="加载"/>
	</enum>
	<enum name="AddControl" desc="加调控制">
		<entry svalue="1" uvalue="1" desc="去调"/>
		<entry svalue="2" uvalue="2" desc="加调"/>
	</enum>
	<enum name="ExtendedJumpOutputMode" desc="加调控制">
		<entry svalue="1" uvalue="1" desc="短码"/>
		<entry svalue="2" uvalue="2" desc="长码"/>
		<entry svalue="3" uvalue="3" desc="交替输出"/>
		<entry svalue="4" uvalue="4" desc="并行输出"/>
	</enum>

	<enum name="CarrierModu" desc="载波调制方式">
		<entry svalue="1" uvalue="1" desc="单载波"/>
		<entry svalue="2" uvalue="2" desc="已调载波"/>
	</enum>
	<enum name="NoiseOutput" desc="噪声源输出">
		<entry svalue="1" uvalue="1" desc="关"/>
		<entry svalue="2" uvalue="2" desc="开"/>
	</enum>
	<enum name="DoppSwitch" desc="多普勒动态模拟开关">
		<entry svalue="1" uvalue="1" desc="关"/>
		<entry svalue="2" uvalue="2" desc="开"/>
	</enum>
</enums>
