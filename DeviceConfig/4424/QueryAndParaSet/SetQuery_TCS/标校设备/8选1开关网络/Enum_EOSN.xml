<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="EOSN" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="2" desc="设备开关电">
		<entry svalue="1" uvalue="1" desc="关电:R"/>
		<entry svalue="2" uvalue="2" desc="开电:G"/>
	</enum>

	<enum name="3" desc="状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	
	<enum name="101" desc="8选1开关">
		<entry svalue="0" uvalue="0" desc="0(表示关断)"/>
		<entry svalue="1" uvalue="1" desc="X跟踪DC1本振"/>
		<entry svalue="2" uvalue="2" desc="X跟踪DC2本振"/>
		<entry svalue="3" uvalue="3" desc="Ka跟踪DC1本振1"/>
		<entry svalue="4" uvalue="4" desc="Ka跟踪DC1本振2"/>
		<entry svalue="5" uvalue="5" desc="Ka跟踪DC2本振1"/>
		<entry svalue="6" uvalue="6" desc="Ka跟踪DC2本振2"/>
		<entry svalue="7" uvalue="7" desc="备用"/>
		<entry svalue="8" uvalue="8" desc="备用"/>
	</enum>
</enums>
