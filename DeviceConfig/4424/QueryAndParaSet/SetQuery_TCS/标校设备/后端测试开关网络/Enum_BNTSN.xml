<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="BNTSN" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="1" uvalue="1" desc="分控"/>
		<entry svalue="2" uvalue="2" desc="本控"/>
	</enum>
	<enum name="2" desc="设备开关电">
		<entry svalue="1" uvalue="1" desc="关电:R"/>
		<entry svalue="2" uvalue="2" desc="开电:G"/>
	</enum>
	<enum name="3" desc="状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	<enum name="FourChooseOneSwitch" desc="4选1开关">
		<entry svalue="1" uvalue="1" desc="Ka频段模拟信号源"/>
		<entry svalue="2" uvalue="2" desc="X频段模拟信号源"/>
		<entry svalue="3" uvalue="3" desc="信号源"/>
		<entry svalue="4" uvalue="4" desc="备份"/>
	</enum>
	<enum name="KDChooseSwitch" desc="宽带选择开关">
		<entry svalue="1" uvalue="1" desc="中心体本振测试"/>
		<entry svalue="2" uvalue="2" desc="主控机房本振及监测"/>
		<entry svalue="3" uvalue="3" desc="备用"/>
		<entry svalue="4" uvalue="4" desc="1.2GHz中频测试"/>
		<entry svalue="5" uvalue="5" desc="10MHz频率参考测试"/>
	</enum>
</enums>
