<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="PDU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ShaftEncodStatus" desc="轴角编码器状态">		
		<entry svalue="1" uvalue="1" desc="正常:G" />
		<entry svalue="2" uvalue="2" desc="故障:R" />
	</enum>
     <enum name="OperatMode" desc="轴角编码器工作模式">              	
        <entry svalue="1" uvalue="1" desc="ACU控" />
        <entry svalue="2" uvalue="2" desc="PDU控" />
	</enum>
    <enum name="TimeFreqState" desc="时频状态">                	
        <entry svalue="1" uvalue="1" desc="内时统" />
        <entry svalue="2" uvalue="2" desc="外时统" />
        	</enum>
</enums>
