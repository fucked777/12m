<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="SPU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="AbgularProceCode" desc="测角数据处理标志码">
		<entry svalue="1" uvalue="1" desc="PDXP" />
		<entry svalue="2" uvalue="2" desc="HDLC" />
	</enum>
	<enum name="AntennaStatus" desc="天线状态">
		<entry svalue="1" uvalue="1" desc="正常:G" />
		<entry svalue="2" uvalue="2" desc="故障:R" />
	</enum>
    <enum name="OnLineMachine" desc="本机在线状态">
        <entry svalue="0" uvalue="0" desc="无此参数" />
        <entry svalue="1" uvalue="1" desc="在线" />
        <entry svalue="2" uvalue="2" desc="备份" />
	</enum>
    <enum name="ObscurState" desc="遮蔽状态">
        <entry svalue="0" uvalue="0" desc="无此参数" />
        <entry svalue="1" uvalue="1" desc="遮蔽" />
        <entry svalue="2" uvalue="2" desc="未遮蔽" />
    </enum>
   	<enum name="SystemWorkMode" desc="系统工作方式">
    	<entry svalue="0" uvalue="0" desc="无此参数" />
    	<entry svalue="1" uvalue="1" desc="S频段" />
    	<entry svalue="2" uvalue="2" desc="X频段" />
		<entry svalue="3" uvalue="3" desc="Ka频段" />
		<entry svalue="4" uvalue="4" desc="S+X频段" />
		<entry svalue="5" uvalue="5" desc="S+Ka频段" />
	</enum>

	<enum name="SystemSelfSet" desc="系统自跟设置">
    	<entry svalue="0" uvalue="0" desc="无此参数" />
    	<entry svalue="1" uvalue="1" desc="S频段" />
    	<entry svalue="2" uvalue="2" desc="X频段" />
		<entry svalue="3" uvalue="3" desc="Ka频段" />
		<entry svalue="4" uvalue="4" desc="S+X频段" />
		<entry svalue="5" uvalue="5" desc="S+Ka频段" />
	</enum>
	<enum name="AngularSamplRate" desc="测角采样率">
    	<entry svalue="0" uvalue="0" desc="无此参数" />
    	<entry svalue="1" uvalue="1" desc="1次/秒" />
    	<entry svalue="2" uvalue="2" desc="4次/秒" />
		<entry svalue="3" uvalue="3" desc="10次/秒" />
    	<entry svalue="4" uvalue="4" desc="20次/秒" />
    	<entry svalue="5" uvalue="5" desc="0.2次/秒" />
		<entry svalue="6" uvalue="6" desc="2次/秒" />
	</enum>   
	<enum name="AngleDataCorrect" desc="角度数据修正">
    	<entry svalue="0" uvalue="0" desc="无此参数" />
    	<entry svalue="1" uvalue="1" desc="修正" />
    	<entry svalue="2" uvalue="2" desc="不修正" />
		<entry svalue="3" uvalue="3" desc="自动选择" />
	</enum>  
	<enum name="ACU" desc="ACU">
    	<entry svalue="0" uvalue="0" desc="无此参数" />
    	<entry svalue="1" uvalue="1" desc="人工" />
    	<entry svalue="2" uvalue="2" desc="自动" />
	</enum>
</enums>
