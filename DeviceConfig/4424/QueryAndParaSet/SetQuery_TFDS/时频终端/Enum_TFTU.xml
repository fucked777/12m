<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="TFTU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="1" uvalue="1" desc="本控"/>
		<entry svalue="2" uvalue="2" desc="分控"/>
	</enum>	
	<enum name="CurrentSource" desc="当前参考源">
		<entry svalue="1" uvalue="1" desc="内时统"/>
		<entry svalue="2" uvalue="2" desc="GPS授时"/>
		<entry svalue="3" uvalue="3" desc="北斗授时"/>
		<entry svalue="4" uvalue="4" desc="BDC1授时"/>
		<entry svalue="5" uvalue="5" desc="BDC2授时"/>
		<entry svalue="6" uvalue="6" desc="BAC授时"/>
	</enum>
	<enum name="OutFreqStatus" desc="外频标状态">
		<entry svalue="1" uvalue="1" desc="可用"/>
		<entry svalue="2" uvalue="2" desc="不可用"/>
	</enum>
	<enum name="Lock" desc="是否锁定">
		<entry svalue="1" uvalue="1" desc="锁定:G"/>
		<entry svalue="2" uvalue="2" desc="失锁:R"/>
	</enum>
	<enum name="PowerStatus" desc="冗余电源状态">
		<entry svalue="1" uvalue="1" desc="单电源故障"/>
		<entry svalue="2" uvalue="2" desc="双电源工作正常"/>
	</enum>
	<enum name="Status" desc="状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>


	<enum name="TimeSynMode" desc="状态">
		<entry svalue="1" uvalue="1" desc="自动"/>
		<entry svalue="2" uvalue="2" desc="仅使用GPS"/>
		<entry svalue="3" uvalue="3" desc="仅使用北斗"/>
		<entry svalue="4" uvalue="4" desc="仅使用外部BDC1"/>
		<entry svalue="5" uvalue="5" desc="仅使用外部BDC2"/>
		<entry svalue="6" uvalue="6" desc="仅使用外部BAC"/>

	</enum>

	<enum name="PinBiaoStatus" desc="状态">
		<entry svalue="1" uvalue="1" desc="内频标"/>
		<entry svalue="2" uvalue="2" desc="外频标"/>
	</enum>
	<enum name="BDCTime" desc="状态">
		<entry svalue="1" uvalue="1" desc="UTC时"/>
		<entry svalue="2" uvalue="2" desc="北京时"/>
	</enum>
	<enum name="TimeChoose" desc="时统选择">
		<entry svalue="1" uvalue="1" desc="内时统"/>
		<entry svalue="2" uvalue="2" desc="外时统"/>
	</enum>
	
</enums>
