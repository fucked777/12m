<?xml version="1.0" encoding="utf-8"?>
<!--
文件名称:有模式时,单元类型+模式类型  无模式:0xFFFF
UnitType:单元类型
ModeType:模式类型
svalue 表示原始值 -设备端发送的原始值
uvalue 表示使用值 -服务端与客户端通用的值(抽象值)
desc   描述
-->
<enums UnitType="TCSU" ModeType="0xFFFF" edition="1.1.1.1">
	<enum name="ControlMode" desc="监控方式">
		<entry svalue="1" uvalue="1" desc="本控"/>
		<entry svalue="2" uvalue="2" desc="分控"/>
	</enum>

	<enum name="LockStatus1" desc="锁定状态">
		<entry svalue="1" uvalue="1" desc="正常:G"/>
		<entry svalue="2" uvalue="2" desc="异常:R"/>
	</enum>
	<enum name="LockStatus2" desc="锁定状态">
		<entry svalue="1" uvalue="1" desc="有信号:G"/>
		<entry svalue="2" uvalue="2" desc="无信号:R"/>
	</enum>
	<enum name="LockStatus3" desc="锁定状态">
		<entry svalue="1" uvalue="1" desc="有效:G"/>
		<entry svalue="2" uvalue="2" desc="无效:R"/>
	</enum>

	<enum name="SignalLockStatus3" desc="锁定状态">
		<entry svalue="1" uvalue="1" desc="外部B码(DC1)"/>
		<entry svalue="2" uvalue="2" desc="外部B码(DC2)"/>
	</enum>


	<enum name="SignalChoose" desc="信号选择">
		<entry svalue="1" uvalue="1" desc="自动选择"/>
		<entry svalue="2" uvalue="2" desc="外部B码(DC1)"/>
		<entry svalue="3" uvalue="3" desc="外部B码(DC2)"/>
	</enum>


	
</enums>
