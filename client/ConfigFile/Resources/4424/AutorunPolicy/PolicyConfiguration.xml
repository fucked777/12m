<?xml version="1.0" encoding="utf-8"?>
<Configure name="自动化运行策略配置">
    <item id="TS"       name="故障处理" type="Enum" enumValue="人工=1|自动=2"/>
    <item id="SCM"      name="系统捕获方式" type="Enum" enumValue="传统=1|同时=2"/>
    <item id="PST"      name="参数命令超时时间(秒)" type="Integer" min="0" max="30" />
    <item  name="校相配置">
        <item id="TCM"      name="目标捕获方式" type="Enum" enumValue="引导捕获=1|等待捕获=2"/>
        <item id="PCaM"     name="校相模式" type="Enum" enumValue="对星快速校相=2" />
        <item id="CaM"      name="标校模式" type="Enum" enumValue="顺序标校=1|集中标校=2"/>
        <item  name="自动化配置">
            <item id="PCaT"     name="校相超时时间(秒)" type="Integer" min="0" max="300" />
        </item>
        <item  name="S测控校相修正">
            <item id="S_CrossLowLimit" name="交叉耦合下限值" type="Integer" min="1" max="255" />
            <item id="S_StandSensit" name="定向灵敏度标准值" type="Integer" min="1" max="65535" />
            <item id="S_DirectToler" name="定向灵敏度允差" type="Integer" min="1" max="65535" />
        </item>
    	<item  name="Ka数传校相修正">
            <item id="KADT_CrossLowLimit" name="交叉耦合下限值" type="Integer" min="1" max="255" />
            <item id="KADT_StandSensit" name="定向灵敏度标准值" type="Integer" min="1" max="65535" />
            <item id="KADT_DirectToler" name="定向灵敏度允差" type="Integer" min="1" max="65535" />
        </item>

    </item>
    <item  name="校零配置"> 
        <item id="ZCaM"     name="校零模式" type="Enum" enumValue="偏馈校零=2" />
        <item  name="自动化配置">
            <item id="ZCaT"     name="校零超时时间(秒)" type="Integer" min="0" max="255" />
      </item>
        <item  name="修正值配置">
            <item id="STTCXZ"   name="TTC修正值(米)" type="Integer" min="0" max="1000" />
            <item id="SKUO2XZ"  name="S扩频修正值(米)" type="Integer" min="0" max="1000" />
            <item id="SYTHSMJXZ"  name="一体化上面级(米)" type="Integer" min="0" max="1000" />
            <item id="SYTHWXXZ"  name="一体化卫星(米)" type="Integer" min="0" max="1000" />
            <item id="SYTHGMLXZ"  name="一体化高码率(米)" type="Integer" min="0" max="1000" />
            <item id="SYTHSMJK2GZBXZ"  name="一体化上面级+扩二共载波(米)" type="Integer" min="0" max="1000" />
            <item id="SYTHSMJK2BGZBXZ"  name="一体化上面级+扩二不共载波(米)" type="Integer" min="0" max="1000" />
            <item id="SYTHWXSK2XZ"  name="一体化卫星+扩二(米)" type="Integer" min="0" max="1000" />
            <item id="SYTHGMLSK2XZ"  name="一体化高码率+扩二(米)" type="Integer" min="0" max="1000" />
            <item id="KTXZ" name="扩跳修正值(米)" type="Integer" min="0" max="1000" />
        </item>
   
    </item>
</Configure>