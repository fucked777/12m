<!-- 
注意:此xml只负责对界面的元素种类进行控制,元素的布局根据界面不同是是写死的
type:
	Enum				下拉列表
	TextLine			单行文本
	TextMultiple		多行文本
	Integer  			整数
	Double				浮点
	Hex					16进制
	IP 					IP地址
-->
<test name="测距随机误差测试">
	<parameter><!-- 测试参数 -->
		<group name="测试计算机参数">
			<item id="SignalSpan" name="频谱仪Span(KHZ)" type="Double" decimals="3" min="1" max="2500000" />
			<item id="SignalRLVL" name="频谱仪RLVL(dBm)" type="Integer" min="-135" max="30" />
		</group>
		<group name="测试统计">
			<item id="StatisPoint" name="统计点数" type="Integer" min="10" max="60000" />
		</group>
	</parameter>
    <result><!-- 结果参数 -->
    </result>
    <bind><!-- 装订参数 -->
    </bind>
</test>
