<?xml version="1.0" encoding="utf-8" ?>
<!-- 
注意:此xml只负责对界面的元素种类进行控制,元素的布局根据界面不同是是写死的
type:
	Enum				下拉列表
	TextLine			单行文本
	TextMultiple		多行文本
	Integer  			整数
	Double				浮点
	Hex					16进制
	IP 					IP地址
-->
<test name="系统捕获时间测试">
	<parameter><!-- 测试参数 -->
		<group name="校零参数">
			<item id="XLFCReduce" name="校零衰减(dB)" type="Integer" min="0" max="80" />
		</group>
		<group name="基带参数">
			<item id="ChanCategory" name="测试通道类别" type="Enum" enumValue="遥测=1|测距=2" />
		</group>
	</parameter>
    <result><!-- 结果参数 -->
    </result>
    <bind><!-- 装订参数 -->
    </bind>
</test>
