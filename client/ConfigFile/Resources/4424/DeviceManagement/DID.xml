<?xml version="1.0" encoding="utf-8" ?>

<!-- 
注意:此xml只负责对界面的元素种类进行控制,元素的布局根据界面不同是是写死的
type:
	Enum				下拉列表
	TextLine			单行文本
	TextMultiple		多行文本
	Integer  			整数
	Double				浮点
	Hex					16进制
	IP 					IP地址
-->
<DID name="数据交互设备">
	<parameter name="数据交互设备参数">
		<item id="DataInteractionIdentification" name="数据交互标识" type="Enum" onlyOne="true" enumValue="DTE1=1|DTE2=2" />
		<item id="Project" name="所属项目" type="Enum" enumValue="TK-4424=TK-4424" />
		<item id="StoreForwardName" name="数据交互名称" type="TextLine" len="20"/>
		<item id="IsUsable" name="是否可用" type="Enum" enumValue="可用=1|不可用=0" />
		<item id="Remarks" name="备注" type="TextMultiple" len="40" important="false"/>
	</parameter>
</DID>
