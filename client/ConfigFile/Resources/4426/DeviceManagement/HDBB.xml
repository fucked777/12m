<?xml version="1.0" encoding="utf-8" ?>

<!-- 
注意:此xml只负责对界面的元素种类进行控制,元素的布局根据界面不同是是写死的
type:
	Enum				下拉列表
	TextLine			单行文本
	TextMultiple		多行文本
	Integer  			整数
	Double				浮点
	Hex					16进制
	IP 					IP地址
-->
<HDBB name="高速基带">
	<parameter name="高速基带参数">
		<item id="MACBIdentification" name="高速基带标识" type="Enum" onlyOne="true" enumValue="HDBB1=1|HDBB2=2" />
		<item id="Project" name="所属项目" type="Enum" enumValue="TK-4426=TK-4426" />
		<item id="MACBName" name="高速基带名称" type="TextLine" len="20"/>
		<item id="IsUsable" name="是否可用" type="Enum" enumValue="可用=1|不可用=0" />
		<item id="Remarks" name="备注" type="TextMultiple" len="40" important="false"/>
	</parameter>
</HDBB>
