<?xml version="1.0" encoding="utf-8" ?>
<!-- 
注意:此xml只负责对界面的元素种类进行控制,元素的布局根据界面不同是是写死的
type:
	Enum				下拉列表
	TextLine			单行文本
	TextMultiple		多行文本
	Integer  			整数
	Double				浮点
	Hex					16进制
	IP 					IP地址
-->
<test name="系统遥测误码率测试">
	<parameter><!-- 测试参数 -->
		<group name="测试参数">
			<item id="TotalSymNum" name="总码元阈值" type="Integer" min="100" max="5000000" />
		</group>
		<group name="S射频开关网络">
			<item id="SBRFSNDecrement" name="增益(dB)" type="Integer" min="0" max="50" />
		</group>
		<group name="S校零变频器">
			<item id="CheckZeroAtten" name="衰减(dB)" type="Integer" min="0" max="60" />
		</group>
	</parameter>
    <result><!-- 结果参数 -->
    </result>
    <bind><!-- 装订参数 -->
    </bind>
</test>
